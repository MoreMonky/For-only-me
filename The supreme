for _,v in next, Ulta_Caliber:GetDescendants() do
	if(v:IsA'BasePart')then
		v.CustomPhysicalProperties = PhysicalProperties.new(0.0001,0,0,0,0)
		v.Massless = true
		v.CanCollide = false
	end
end
