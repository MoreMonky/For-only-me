local dssj3colors = {"Cyan","Institutional white"}
dssj3color = #dssj3colors

function swait(num)
	if num == 0 or num == nil then
		game:service("RunService").Stepped:wait(0)
	else
		for i = 0, num do
			game:service("RunService").Stepped:wait(0)
		end
	end
end


	function thething()
	
	for i = 1,20 do
		Model.Head2.HeadHighLight.OutlineTransparency = Model.Head2.HeadHighLight.OutlineTransparency - 0.05
		Model.Torso.TorsoHighLight.OutlineTransparency = Model.Torso.TorsoHighLight.OutlineTransparency - 0.05
		Model.Torso3.TorsoHighLight3.OutlineTransparency = Model.Torso3.TorsoHighLight3.OutlineTransparency - 0.05
		Model.RA.HighLightRA.OutlineTransparency = Model.RA.HighLightRA.OutlineTransparency - 0.05
		Model.LA.HighLightLA.OutlineTransparency = Model.LA.HighLightLA.OutlineTransparency - 0.05
		Model.LL.HighLightLL.OutlineTransparency = Model.LL.HighLightLL.OutlineTransparency - 0.05
		Model.RL.HighLightRL.OutlineTransparency = Model.RL.HighLightRL.OutlineTransparency - 0.05
		wait()
	end


    wait(3)
    
    
	
			for i = 1,5 do
				coroutine.wrap(function()
					dssj3color = dssj3colors[math.random(1,#dssj3colors)]
			local		orb = Instance.new("Part",owner.Character)
					orb.Size = Vector3.new(0.2,0.2,0.2)
					orb.BrickColor = BrickColor.new(dssj3color)
					orb.Material = "Neon"
					orb.Shape = "Ball"
					orb.Transparency = 1
					orb.Anchored = true
					orb.CanCollide = false
					orb.CFrame = Model.Torso.CFrame * CFrame.new(math.random(-5,5),math.random(-5,5),math.random(-5,5))
					for i = 1, 60 do
						orb.Transparency = orb.Transparency - .05
						
						orb.CFrame = orb.CFrame:lerp(CFrame.new(owner.Character.GenkiTest.Position),.01)
						swait()
					end
					for i = 1, 60 do
						orb.CFrame = orb.CFrame:lerp(CFrame.new(owner.Character.GenkiTest.Position),.1)
						owner.Character.GenkiTest.Size = owner.Character.GenkiTest.Size + Vector3.new(0.002, 0.002, 0.002)
						owner.Character.GenkiTest2.Size = owner.Character.GenkiTest2.Size + Vector3.new(0.001, 0.001, 0.001)
						
						owner.Character.GenkiTest2.Transparency = 0
						owner.Character.GenkiTest.Transparency = 0.2

						swait()
					end
					orb:Remove()
				end)()
				
				wait()
				
	end
	
	wait(1)
	
	
		for i = 1,20 do
		Model.Head2.HeadHighLight.OutlineTransparency = Model.Head2.HeadHighLight.OutlineTransparency + 0.05
		Model.Torso.TorsoHighLight.OutlineTransparency = Model.Torso.TorsoHighLight.OutlineTransparency + 0.05
		Model.Torso3.TorsoHighLight3.OutlineTransparency = Model.Torso3.TorsoHighLight3.OutlineTransparency + 0.05
		Model.RA.HighLightRA.OutlineTransparency = Model.RA.HighLightRA.OutlineTransparency + 0.05
		Model.LA.HighLightLA.OutlineTransparency = Model.LA.HighLightLA.OutlineTransparency + 0.05
		Model.LL.HighLightLL.OutlineTransparency = Model.LL.HighLightLL.OutlineTransparency + 0.05
		Model.RL.HighLightRL.OutlineTransparency = Model.RL.HighLightRL.OutlineTransparency + 0.05
		wait()
	end
	
	
	end
	
	
		
