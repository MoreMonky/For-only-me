local Event = Instance.new("RemoteEvent")
Event.Name = "MouseInp"

Event.Parent = NLS([==[
	local Player = owner
	local Mouse = owner:GetMouse()
	local hb = game:GetService("RunService").Heartbeat
	
	script.Name = "MousePosInp"
	
	local CanDo = true
	
	Mouse.KeyDown:Connect(function(key)
		local p = Mouse.hit.p

		Player.Character.MousePosInp.MouseInp:FireServer(p,key)

	end)
	
	hb:Connect(function()
		local p = Mouse.hit.p
		if CanDo == true then
			CanDo = false
			Player.Character.MousePosInp.MouseInp:FireServer(p,"")
			wait()
			CanDo = true
		end	
	end)
	
]==],owner.Character)


