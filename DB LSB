IT = Instance.new

VV = Vector3.new
MM = {RN = math.random, CS = math.cos, AS = math.acos, RD = math.rad, SN = math.sin}
CC = {NW = CFrame.new, AA = CFrame.Angles}

function Killpunch(Humanoid)
	spawn(function()
		if Humanoid:FindFirstChild("Killed") == nil then
			local Mark = IT("Folder",Humanoid)
			Mark.Name = "Killed"
			local TORSO = Humanoid.Parent:FindFirstChild("Torso") or Humanoid.Parent:FindFirstChild("UpperTorso") or Humanoid.Parent:FindFirstChild("HumanoidRootPart")

			for i,v in pairs(Humanoid:GetChildren()) do

				if v.ClassName == "Script" or v.ClassName == "LocalScript" then
					v:destroy()
				end
				for i,p in pairs(v:GetChildren()) do
					if p.ClassName == "Weld" or p.ClassName == "Motor6D" or p.ClassName == "BodyVelocity" then
						p:destroy()
					end
				end
			end

			for i,v in next, Humanoid:children() do
				if v:IsA("LocalScript") or v:IsA("Script") or v:IsA("ModuleScript") then
					v.Disabled = true
					wait()
					v:destroy()
				end
			end

			spawn(function()
				TORSO.Archivable = false TORSO.Anchored = false
				local FLIGHTFORCE = Instance.new("BodyPosition", TORSO)
				FLIGHTFORCE.D = 215
				FLIGHTFORCE.P = 200
				FLIGHTFORCE.maxForce = VV(40000, 40000, 40000)
				FLIGHTFORCE.position = root.CFrame * Vector3.new(0,0.5,-4)
				Humanoid.Health = Humanoid.Health + -5
				spawn(function()
					TORSO.CFrame = TORSO.CFrame * CC.AA(MM.RD(MM.RN(-30/1.25,30/1.25)),MM.RD(MM.RN(-30/1.25,30/1.25)),MM.RD(MM.RN(-30/1.25,30/1.25)))
					TORSO.RotVelocity = VV(MM.RD(-10*1.75,10*1.75),MM.RD(-10*1.75,10*1.75),MM.RD(-10*1.75,10*1.75))
				end)
				wait(1)
				FLIGHTFORCE:Destroy()
			end)

			spawn(function()
				punchsound:Play()
				wait(2)
				punchsound:Stop()
			end)

			spawn(function()
				wait(0.02)

				Mark:destroy()
			end)

		end
	end)
end

function Killpunch2(Humanoid)
	spawn(function()
		if Humanoid:FindFirstChild("Killed") == nil then
			local Mark = IT("Folder",Humanoid)
			Mark.Name = "Killed"
			local TORSO = Humanoid.Parent:FindFirstChild("Torso") or Humanoid.Parent:FindFirstChild("UpperTorso") or Humanoid.Parent:FindFirstChild("HumanoidRootPart")
			for i,v in pairs(Humanoid:GetChildren()) do
				if v.ClassName == "Script" or v.ClassName == "LocalScript" then
					v:destroy()
				end
				for i,p in pairs(v:GetChildren()) do
					if p.ClassName == "Weld" or p.ClassName == "Motor6D" or p.ClassName == "BodyVelocity" then
						p:destroy()
					end
				end
			end

			for i,v in next, Humanoid:children() do
				if v:IsA("LocalScript") or v:IsA("Script") or v:IsA("ModuleScript") then
					v.Disabled = true
					wait()
					v:destroy()
				end
			end
			spawn(function()
				TORSO.Archivable = false TORSO.Anchored = false

				local FLIGHTFORCE = Instance.new("BodyPosition", TORSO)
				FLIGHTFORCE.D = 215
				FLIGHTFORCE.P = 200
				FLIGHTFORCE.maxForce = VV(40000, 40000, 40000)
				FLIGHTFORCE.position = root.CFrame * Vector3.new(0,0.5,-4)

				Humanoid.Health = Humanoid.Health + -30
				spawn(function()
					TORSO.CFrame = TORSO.CFrame * CC.AA(MM.RD(MM.RN(-30/1.25,30/1.25)),MM.RD(MM.RN(-30/1.25,30/1.25)),MM.RD(MM.RN(-30/1.25,30/1.25)))
					TORSO.RotVelocity = VV(MM.RD(-10*1.75,10*1.75),MM.RD(-10*1.75,10*1.75),MM.RD(-10*1.75,10*1.75))
				end)
				wait(1)
				FLIGHTFORCE:Destroy()
			end)

			spawn(function()
				punchsound:Play()
				wait(2)
				punchsound:Stop()
			end)

			spawn(function()
				wait(0.02)

				Mark:destroy()
			end)

		end
	end)

end

function Killpunch3(Humanoid)
	spawn(function()
		if Humanoid:FindFirstChild("Killed") == nil then
			local Mark = IT("Folder",Humanoid)
			Mark.Name = "Killed"
			local TORSO = Humanoid.Parent:FindFirstChild("Torso") or Humanoid.Parent:FindFirstChild("UpperTorso") or Humanoid.Parent:FindFirstChild("HumanoidRootPart")

			for i,v in pairs(Humanoid:GetChildren()) do
				if v.ClassName == "Script" or v.ClassName == "LocalScript" then
					v:destroy()
				end
				for i,p in pairs(v:GetChildren()) do
					if p.ClassName == "Weld" or p.ClassName == "Motor6D" or p.ClassName == "BodyVelocity" then
						p:destroy()
					end
				end
			end

			for i,v in next, Humanoid:children() do
				if v:IsA("LocalScript") or v:IsA("Script") or v:IsA("ModuleScript") then
					v.Disabled = true
					wait()
					v:destroy()
				end
			end

			Humanoid.Health = Humanoid.Health + -5

			spawn(function()
				punchsound:Play()
				wait(2)
				punchsound:Stop()
			end)

			spawn(function()
				wait(0.02)

				Mark:destroy()
			end)

		end
	end)
end

function AoEpunch(POSITION,RANGE)
	local CHILDREN = workspace:GetDescendants()
	for index, CHILD in pairs(CHILDREN) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				for _, c in pairs(CHILD:GetChildren()) do
					if c:IsA("BasePart") then
						if (c.Position - POSITION).Magnitude <= RANGE+c.Size.Magnitude then
							Killpunch(HUM)
						end
					end
				end
			end
		end
	end
end

function AoEpunch2(POSITION,RANGE)
	local CHILDREN = workspace:GetDescendants()
	for index, CHILD in pairs(CHILDREN) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				for _, c in pairs(CHILD:GetChildren()) do
					if c:IsA("BasePart") then
						if (c.Position - POSITION).Magnitude <= RANGE+c.Size.Magnitude then
							Killpunch2(HUM)
						end
					end
				end
			end
		end
	end
end

function AoEpunch3(POSITION,RANGE)
	local CHILDREN = workspace:GetDescendants()
	for index, CHILD in pairs(CHILDREN) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				for _, c in pairs(CHILD:GetChildren()) do
					if c:IsA("BasePart") then
						if (c.Position - POSITION).Magnitude <= RANGE+c.Size.Magnitude then
							Killpunch3(HUM)
						end
					end
				end
			end
		end
	end
end


function CreateEffect(Parent, EffectType, EffectColor)

	if EffectType == "BarrageArm" then

		local FadeArm = Parent:Clone()
		for i,v in pairs(FadeArm:GetDescendants()) do
			if v.Name ~= "LAOut" and v.Name ~= "RAOut" and v:IsA("Weld") == false and v:IsA("SpecialMesh") == false then
				v:Destroy()
			end
			if v:IsA("ParticleEmitter") then
				v:Destroy()
			end
		end
		FadeArm.Anchored = true
		FadeArm.Color = EffectColor
		FadeArm.Material = Enum.Material.SmoothPlastic
		FadeArm.CFrame = FadeArm.CFrame * CFrame.new(math.random(-1,1), -1.3, math.random(-1.5,1.5))
		FadeArm.Name = "Test"
		FadeArm.Parent = char
		FadeArm.Transparency = 0

		coroutine.resume(coroutine.create(function()
			for i = 1,50 do
				game:GetService("RunService").Heartbeat:Wait()
				FadeArm.Transparency = FadeArm.Transparency + 0.06
				FadeArm.CFrame = FadeArm.CFrame:lerp(FadeArm.CFrame * CFrame.new(0, -0.45, -0), 0.4)
				for Index, Child in next, FadeArm:GetDescendants() do
					if Child:IsA("BasePart") then
						Child.Transparency = FadeArm.Transparency
					end
				end
			end
			FadeArm:Destroy()
		end))
	end
end

function CreateEffect3(Parent, EffectType, EffectColor)

	if EffectType == "BarrageArm" then

		local	FadeArm = Parent:Clone()
		FadeArm.Anchored = true
		FadeArm.Color = EffectColor
		FadeArm.Material = Enum.Material.SmoothPlastic
		FadeArm.CFrame = FadeArm.CFrame
		FadeArm.Name = "Test"
		FadeArm.CanCollide = false
		FadeArm.Parent = char

		FadeArm.Transparency = 0

		local	torso1 = Instance.new("Part",char)
		torso1.Size = Vector3.new(2,2,1)
		torso1.Anchored = true
		torso1.Rotation = Vector3.new(0,180,0)
		torso1.CFrame = torso.CFrame
		torso1.CanCollide = false
		torso1.BrickColor = BrickColor.new("Institutional white")
		torso1.Name = "Torso2"

		local	head1 = Instance.new("Part",char)
		head1.Size = Vector3.new(1.25,1.25,1.25)
		head1.BrickColor = BrickColor.new("Institutional white")
		head1.Name = "Head2"
		head1.Anchored = true
		head1.CanCollide = false
		head1.CFrame = hed.CFrame

		local	headmesh1 = Instance.new("SpecialMesh",head1)
		headmesh1.MeshType = "Head"

		local	larm1 = Instance.new("Part",char)
		larm1.Size = Vector3.new(1,2,1)
		larm1.BrickColor = BrickColor.new("Institutional white")
		larm1.Name = "Left Arm2"
		larm1.CFrame = larm.CFrame
		larm1.Anchored = true
		larm1.CanCollide = false

		local	rarm1 = Instance.new("Part",char)
		rarm1.Size = Vector3.new(1,2,1)
		rarm1.BrickColor = BrickColor.new("Institutional white")
		rarm1.Name = "Right Arm2"
		rarm1.CFrame = rarm.CFrame
		rarm1.CanCollide = false
		rarm1.Anchored = true

		local	lleg1 = Instance.new("Part",char)
		lleg1.Size = Vector3.new(1,2,1)
		lleg1.Name = "Left Leg2"
		lleg1.BrickColor = BrickColor.new("Institutional white")
		lleg1.CFrame = lleg.CFrame
		lleg1.CanCollide = false
		lleg1.Anchored = true

		local	rleg1 = Instance.new("Part",char)
		rleg1.Size = Vector3.new(1,2,1)
		rleg1.Name = "Right Leg2"
		rleg1.CFrame = rleg.CFrame
		rleg1.BrickColor = BrickColor.new("Institutional white")
		rleg1.CanCollide = false
		rleg1.Anchored = true

		coroutine.resume(coroutine.create(function()
			for i = 1,50 do
				game:GetService("RunService").Heartbeat:Wait()
				FadeArm.Transparency = FadeArm.Transparency + 0.06
				torso1.Transparency = torso1.Transparency + 0.06
				head1.Transparency = head1.Transparency + 0.06
				rarm1.Transparency = rarm1.Transparency + 0.06
				larm1.Transparency = larm1.Transparency + 0.06
				lleg1.Transparency = lleg1.Transparency + 0.06
				rleg1.Transparency = rleg1.Transparency + 0.06
				for Index, Child in next, FadeArm:GetDescendants() do
					if Child:IsA("BasePart") then
						Child.Transparency = FadeArm.Transparency
					end
				end
			end
			FadeArm:Destroy()
			torso1:Destroy()
			head1:Destroy()
			rarm1:Destroy()
			larm1:Destroy()
			lleg1:Destroy()
			rleg1:Destroy()
		end))
	end
end

function HighSpeed()
	attack = true

	local tempattachment = Instance.new("Attachment",root)
	tempattachment.Position = Vector3.new(0,0,-4)

	for i=1,10 do rs:wait()
		root.Velocity = Vector3.new(0,0,0)
		root.CFrame = CFrame.new(tempattachment.WorldPosition) * CFrame.Angles(math.rad(root.Orientation.X),math.rad(root.Orientation.Y),math.rad(root.Orientation.Z))
		CreateEffect3(a8, "BarrageArm", Color3.new(1, 1, 1))
		AoEpunch(root.CFrame * Vector3.new(0,0.5,-4),3.75)
	end

	tempattachment:Destroy()

	attack = false
	delay(0.1,function() end)
end

Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "e" then
		if punch1 == true and combo1 == false and punch2 == false and attack == false then
			attack = true
			punch1 = false
			combo1 = true
			punch2 = false
			hum.WalkSpeed = 0
			hum.JumpPower = 0
			for i = 0, 1.35, 0.1 do
				swait()
				rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, 0.7, -0.3) * angles(math.rad(-4-2*i), math.rad(4+2*i), math.rad(-40-11*i)), 0.2)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(0), math.rad(0), math.rad(40+11*i)), 0.2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.6, 0.2) * angles(math.rad(90+4*i), math.rad(-43), math.rad(16+6*i)), 0.3)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(90), math.rad(0), math.rad(-43)), 0.3)
				RH.C0 = clerp(RH.C0, CFrame.new(1, -0.7, 0) * RHCF * angles(math.rad(-34), math.rad(0), math.rad(-17)), 0.2)
				LH.C0 = clerp(LH.C0, CFrame.new(-1, -0.7, -0.2) * LHCF * angles(math.rad(-24), math.rad(0), math.rad(0)), 0.2)
			end

			AoEpunch(root.CFrame * Vector3.new(0,0.5,-4),3.75)
			for i = 0, 1.12, 0.1 do
				swait()
				rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, -0.9, -0) * angles(math.rad(14), math.rad(6), math.rad(23)), 0.35)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(-4), math.rad(0), math.rad(-23)), 0.35)
				RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.6, -0.8) * angles(math.rad(110), math.rad(23), math.rad(2)), 0.4)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0.2) * angles(math.rad(-37), math.rad(0), math.rad(-13)), 0.35)
				RH.C0 = clerp(RH.C0, CFrame.new(1, -1, -0.3) * RHCF * angles(math.rad(-4), math.rad(0), math.rad(6)), 0.3)
				LH.C0 = clerp(LH.C0, CFrame.new(-1, -1, 0.05) * LHCF * angles(math.rad(-22), math.rad(0), math.rad(23)), 0.3)
			end
			hum.WalkSpeed = 16
			hum.JumpPower = 50
			attack = false
		elseif punch1 == false and combo1 == true and punch2 == false and attack == false then
			attack = true
			punch1 = false
			combo1 = false
			punch2 = true
			hum.WalkSpeed = 0
			hum.JumpPower = 0
			for i = 0, 1.65, 0.1 do
				swait()
				root.Velocity = root.CFrame.lookVector * 0

				rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, 0.7, -0.3) * angles(math.rad(-32), math.rad(-2), math.rad(90)), 0.2)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(0), math.rad(-17), math.rad(-90)), 0.2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.1, 0.5, -0.6) * angles(math.rad(90), math.rad(0), math.rad(-56)), 0.3)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 0.6, -0.5) * angles(math.rad(90), math.rad(0), math.rad(56)), 0.3)
				RH.C0 = clerp(RH.C0, CFrame.new(1, .62 , -0.3) * RHCF * angles(math.rad(-40), math.rad(0), math.rad(2)), 0.2)
				LH.C0 = clerp(LH.C0, CFrame.new(-1, -0.7, 0) * LHCF * angles(math.rad(-28), math.rad(0), math.rad(0)), 0.2)
			end
			for i = 1, 50 do 
				swait()

				root.Velocity = root.CFrame.lookVector * 10
				rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, -0.5, -0.3) * angles(math.rad(-44), math.rad(-2), math.rad(90)), 0.7)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(0), math.rad(-24), math.rad(-90)), 0.7)
				RW.C0 = clerp(RW.C0, CFrame.new(1.1, 0.5, -0.6) * angles(math.rad(90), math.rad(0), math.rad(-56)), 0.7)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 0.6, -0.5) * angles(math.rad(90), math.rad(0), math.rad(56)), 0.7)
				RH.C0 = clerp(RH.C0, CFrame.new(1, -.6 , 0) * RHCF * angles(math.rad(math.random(-100,-10)), math.rad(0), math.rad(2)), 0.7)
				LH.C0 = clerp(LH.C0, CFrame.new(-1, -0.7, 0) * LHCF * angles(math.rad(-34), math.rad(0), math.rad(0)), 0.7)
				AoEpunch(root.CFrame * Vector3.new(0,0.5,-4),3.75)
			end
			for i = 1, 10 do 
				swait()
				rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, 0.7, -0.3) * angles(math.rad(-32), math.rad(-2), math.rad(90)), 0.2)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(0), math.rad(-17), math.rad(-90)), 0.2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.1, 0.5, -0.6) * angles(math.rad(90), math.rad(0), math.rad(-56)), 0.3)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 0.6, -0.5) * angles(math.rad(90), math.rad(0), math.rad(56)), 0.3)
				RH.C0 = clerp(RH.C0, CFrame.new(1, .62 , -0.3) * RHCF * angles(math.rad(-40), math.rad(0), math.rad(2)), 0.2)
				LH.C0 = clerp(LH.C0, CFrame.new(-1, -0.7, 0) * LHCF * angles(math.rad(-28), math.rad(0), math.rad(0)), 0.2)
			end
			hum.WalkSpeed = 16
			hum.JumpPower = 50
			attack = false
		elseif punch1 == false and combo1 == false and punch2 == true and attack == false then
			attack = true
			punch1 = true
			combo1 = false
			punch2 = false
			hum.WalkSpeed = 0
			hum.JumpPower = 0
			for i=0, 0.1, 0.1 / Animation_Speed do
				swait()
				rootj.C0 = clerp(rootj.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(7), RAD(90)), 1.8 / Animation_Speed)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(--[[-9 - 1]]0 * math.cos(sine / 25)), math.rad(0), math.rad(-70)), 0.1)
				RW.C0 = clerp(RW.C0, CF(1.5 -0.30, 0.4, 0) * ANGLES(RAD(155), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
				LW.C0 = clerp(LW.C0, CF(-1.5 +0.30, 0.1, 0) * ANGLES(RAD(60), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 2 / Animation_Speed)
				RH.C0 = clerp(RH.C0, CF(1.7-0.10, -1 +0.40, -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(-60), RAD(-5)), 1.8 / Animation_Speed)
				LH.C0 = clerp(LH.C0, CF(-1+0.10, -1 +0.40, -0.5) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-15), RAD(25), RAD(0)), 1.8 / Animation_Speed)
			end
			AoEpunch2(root.CFrame * Vector3.new(0,0.5,-4),3.75)
			for i=0, 1, 0.1 / Animation_Speed do
				swait()
				rootj.C0 = clerp(rootj.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(7), RAD(90)), 1.8 / Animation_Speed)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(-9 - 1 * math.cos(sine / 25)), math.rad(0), math.rad(-70)), 0.1)
				RW.C0 = clerp(RW.C0, CF(1.5 -0.30, 0.4, 0) * ANGLES(RAD(155), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
				LW.C0 = clerp(LW.C0, CF(-1.5 +0.30, 0.1, 0) * ANGLES(RAD(60), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 2 / Animation_Speed)
				RH.C0 = clerp(RH.C0, CF(1.7-0.10, -1 +0.40, -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(-60), RAD(-5)), 1.8 / Animation_Speed)
				LH.C0 = clerp(LH.C0, CF(-1+0.10, -1 +0.40, -0.5) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-15), RAD(25), RAD(0)), 1.8 / Animation_Speed)
			end
			hum.WalkSpeed = 16
			hum.JumpPower = 50
			attack = false
		end
	end

end)

local kiblast1 = true
local kiblast2 = false

function ExplosionKiBlast(Part)

	local Ball = Instance.new("Part", Part)
	Ball.Size = Vector3.new(1,1,1)
	Ball.Shape = "Ball"
	Ball.Transparency = 0
	Ball.Anchored = true
	Ball.CanCollide = false
	Ball.Position = Part.Position
	Ball.BrickColor = BrickColor.new("New Yeller")
	Ball.Material = "Neon"
	Ball.Size = Ball.Size + Vector3.new(10,10,10)
	Ball.Transparency = 0.1
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.2
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.3
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.4
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.5
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.6
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.7
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.8
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 0.9
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball.Transparency = 1
	Ball.Size = Ball.Size + Vector3.new(1,1,1)

	wait(0.025)

	Ball:Destroy()

end

Cos = math.cos
Acos = math.acos
Sin = math.sin
cos = math.cos
Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "r" and kiblast1 == true and kiblast2 == false and attack == false then
		kiblast1 = false
		kiblast2 = true
		char.HumanoidRootPart.Anchored = true

		attack = true
		local Ball = Instance.new("Part", char)
		Ball.Size = Vector3.new(1,1,1)
		Ball.Shape = "Ball"
		Ball.Anchored = true
		Ball.BrickColor = BrickColor.new("New Yeller")
		Ball.CFrame = char["Left Arm"].CFrame
		Ball.CanCollide = false
		Ball.Material = "Neon"
		Ball.Name = "FVX"
		Ball.Transparency = 1

		for i = 1, 10 do
			Ball.CFrame = char["Left Arm"].CFrame
			swait()
			lerpz(RJ, "C0", RJC0 * cf(0, 0, 0.03 * cos(sine / 60)) * ang(rd(0), rd(0), rd(0)), 0.3)
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(3 - 3 * cos(sine / 60)), rd(0), rd(0)), 0.3)
			RW.C0 = clerp(RW.C0, CF(1.5, 0.5, 1) * ang(rd(90 - 2 * cos(sine / 60)), rd(1 - 2 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			LW.C0 = clerp(LW.C0, CF(-1.5, 0.5, -1) * ang(rd(90 - 2 * cos(sine / 60)), rd(1 - 2 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(RH, "C0", RHC0 * cf(0, 0, 0) * ang(rd(-3 - 1 * cos(sine / 60)), rd(-5 - 1 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(RH, "C1", RHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.3)
			lerpz(LH, "C0", LHC0 * cf(0, 0, 0) * ang(rd(-3 - 1 * cos(sine / 60)), rd(5 - 1 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(LH, "C1", LHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.3)
		end
		char.HumanoidRootPart.Anchored = false
		attack = false
		local exploded = false
		Ball.Touched:connect(function(hit)
			if exploded == false  then
				exploded = true
				wait(0.7)
				Ball.Transparency = 1
				ExplosionKiBlast(Ball)
				Ball:Destroy()
			end
		end)
		Ball.Transparency = 0
		for i = 1, 300 do
			swait()
			Ball.CFrame = Ball.CFrame * CFrame.new(0, -1.5, 0 * 100)
			AoEpunch(Ball.CFrame * Vector3.new(0,0.5,-4),3.75)
		end
		Ball:Destroy()
	elseif key == "r" and kiblast1 == false and kiblast2 == true and attack == false then
		kiblast1 = true
		kiblast2 = false
		attack = true
		char.HumanoidRootPart.Anchored = true

		local Ball = Instance.new("Part", char)
		Ball.Size = Vector3.new(1,1,1)
		Ball.Shape = "Ball"
		Ball.Anchored = true
		Ball.BrickColor = BrickColor.new("New Yeller")
		Ball.CFrame = char["Right Arm"].CFrame
		Ball.CanCollide = false
		Ball.Material = "Neon"
		Ball.Name = "FVX"
		Ball.Transparency = 1

		for i = 1, 10 do
			Ball.CFrame = char["Right Arm"].CFrame
			swait()
			lerpz(RJ, "C0", RJC0 * cf(0, 0, 0.03 * cos(sine / 60)) * ang(rd(0), rd(0), rd(0)), 0.3)
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(3 - 3 * cos(sine / 60)), rd(0), rd(0)), 0.3)
			RW.C0 = clerp(RW.C0, CF(1.5, 0.5, -1) * ang(rd(90 - 2 * cos(sine / 60)), rd(1 - 2 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			LW.C0 = clerp(LW.C0, CF(-1.5, 0.5, 1) * ang(rd(90 - 2 * cos(sine / 60)), rd(1 - 2 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(RH, "C0", RHC0 * cf(0, 0, 0) * ang(rd(-3 - 1 * cos(sine / 60)), rd(-5 - 1 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(RH, "C1", RHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.3)
			lerpz(LH, "C0", LHC0 * cf(0, 0, 0) * ang(rd(-3 - 1 * cos(sine / 60)), rd(5 - 1 * cos(sine / 60)), rd(1 * cos(sine / 60))), 0.3)
			lerpz(LH, "C1", LHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.3)
		end
		attack = false
		char.HumanoidRootPart.Anchored = false
		local exploded2 = false
		Ball.Transparency = 0
		Ball.Touched:connect(function(hit)
			if exploded2 == false then
				exploded2 = true
				wait(0.7)
				Ball.Transparency = 1
				ExplosionKiBlast(Ball)
				Ball:Destroy()
			end
		end)
		Ball.Transparency = 0
		for i = 1, 300 do
			swait()
			Ball.CFrame = Ball.CFrame * CFrame.new(0, -1.5, 0 * 100)
		end
		Ball:Destroy()
	end
end)

TextLabel22.Text = "Script Status : Loaded"
TextLabel22.TextColor = BrickColor.new("Flame yellowish orange")
TextLabel22.TextColor3 = Color3.new(0.121569, 0.976471, 0.14902)

char.Humanoid.Died:Connect(function()

	plr.PlayerGui.ScreenGui:Destroy()
	script:Destroy()

end)
