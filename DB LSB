TailParts = {char.Torso}
Welds = {}

for i = 2, 23 do
	TailParts[i] = Instance.new("Part", char)
	TailParts[i].Name = "TailPart"
	TailParts[i].CanCollide = false
	TailParts[i].Locked = true
	TailParts[i].Transparency = 1
	TailParts[i].formFactor = "Symmetric"
	TailParts[i].Size = Vector3.new(0.5, 0.5, 0.5)
	TailParts[i].Shape = "Cylinder"
	TailMesh = Instance.new("SpecialMesh", TailParts[i])
	TailMesh.Name = "TailMesh"
	TailMesh.Scale = Vector3.new((0.3 - 22) / 80, (0.3 - 22) / 80, 0.29)
	TailMesh.MeshId = "rbxassetid://10873612680"
	TailMesh.Offset = Vector3.new(0, 0, 0) --* CFrame.Angles(0, math.rad(90), 0)



	Welds[i - 1] = Instance.new("Weld", TailParts[i])
	Welds[i - 1].Part0 = TailParts[i - 1]
	Welds[i - 1].Part1 = TailParts[i]
	Welds[i - 1].C0 = CFrame.new(0, 0, 0.125) --* CFrame.Angles(0, math.rad(90), 0)
	Welds[i - 1].C1 = CFrame.new(0, 0, -0.125) --* CFrame.Angles(0, math.rad(180), 0)

end
Welds[1].C0 = CFrame.new(0, -1, 0.5)
--------------------
--Part Coloration--
--------------------
for i = 2, 23 do
	TailParts[i].BrickColor = BrickColor.new("Crimson")
end
TailParts[23].BrickColor = BrickColor.new("Crimson")

