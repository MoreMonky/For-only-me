		Part199.CanCollide = false
		Part199.BrickColor = BrickColor.new("Cyan")
		Part199.Material = "Neon"
		Part199.CastShadow = false
		Part199.Size = Vector3.new(2,2,2)
		Part199.Transparency = 0.5
		Part199.Shape = "Ball"
		local weld = Instance.new("Weld", char["Left Arm"])
		weld.Part0 = char["Left Arm"]
		weld.Part1 = Part
		weld.Name = "KAMEWELD"
		weld.C0 = CFrame.new(0,-1,0.5)
		local weld2 = Instance.new("Weld", Part)
		weld2.Part0 = Part
		weld2.Part1 = Part199
		weld2.C0 = CFrame.new(0,0,0)
		local S6 = Instance.new("Sound",HumanoidRootPart)
		S6.SoundId = "rbxassetid://2889440510"
		S6.Volume = 1
		S6:Play()
