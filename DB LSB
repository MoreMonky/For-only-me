--kamehameha
---------------------------------------------------------------------

Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "q" and ssj4lb == false and FusedSSJ4 == false then
		if canattack == true then
			canattack = false
			CanFight = false
			canwalk = false
			attack = true
			kamefire.SoundId = "rbxassetid://2889601935"
			kamecharge.SoundId = "rbxassetid://2889601147"

			TestKame()
			attack = false
			canattack = true
			canwalk = true
			CanFight = true
		end
	elseif key == "j" and SSJ3 == true and OnePiece == 560 then
		if canattack == true then
			OnePiece = 0
			EndOfTheRun = false

			canattack = false
			CanFight = false
			canwalk = false
			attack = true

			BaseGauge.Image = "rbxassetid://11531040002"
			Gauge.Image = "rbxassetid://11531040002"

			BillboardGaugeImage2.Image = "rbxassetid://11531040002"
			BillboardGaugeImage.Image = "rbxassetid://11531040002"

			UltraKamehameha()
			attack = false
			canattack = true
			canwalk = true
			CanFight = true
		end
	elseif key == "q" and ssj4lb == true then
		if canattack == true then
			canattack = false
			CanFight = false
			canwalk = false
			attack = true
			TestKame(BrickColor.new("Really red"),"rbxassetid://5893566017","rbxassetid://0",3,20,false)
			attack = false
			canattack = true
			canwalk = true
			CanFight = true
		end
	elseif key == "q" and FusedSSJ4 == true then
		if canattack == true then
			canattack = false
			attack = true
			canidle = false
			canwalk = false
			BigBangKamehameha()
			attack = false
			canattack = true
			canwalk = true
			CanFight = true
		end
	elseif key == "j" and FusedSSJ4 == true then
		if canattack == true then
			canattack = false
			attack = true
			canidle = false
			canwalk = false
			--	BigBangKamehameha()
			local		Gogeta1 = Instance.new("Model",char.Torso)
			Gogeta1.Name = "Gogeta1"

			humanoid1 = Instance.new("Humanoid",char.Torso.Gogeta1)
			humanoid1.DisplayDistanceType = "None"

			shirt1 = Instance.new("Shirt",char.Torso.Gogeta1)
			shirt1.Name = "Shirt"

			pants1 = Instance.new("Pants",char.Torso.Gogeta1)
			pants1.Name = "Pants"

			torso1 = Instance.new("Part",char.Torso.Gogeta1)
			torso1.Size = Vector3.new(2,2,1)
			torso1.Rotation = Vector3.new(0,180,0)
			torso1.Position = Vector3.new(0,4,0)
			torso1.BrickColor = BrickColor.new("Pastel brown")
			torso1.Name = "Torso"

			head1 = Instance.new("Part",char.Torso.Gogeta1)
			head1.Size = Vector3.new(1.25,1.25,1.25)
			head1.BrickColor = BrickColor.new("Pastel brown")
			head1.Name = "Head"
			head1.Position = Vector3.new(0,6,0)

			headmesh1 = Instance.new("SpecialMesh",head1)
			headmesh1.MeshType = "Head"

			larm1 = Instance.new("Part",char.Torso.Gogeta1)
			larm1.Size = Vector3.new(1,2,1)
			larm1.BrickColor = BrickColor.new("Pastel brown")
			larm1.Name = "Left Arm"
			larm1.Position = Vector3.new(-2,4,0)

			rarm1 = Instance.new("Part",char.Torso.Gogeta1)
			rarm1.Size = Vector3.new(1,2,1)
			rarm1.BrickColor = BrickColor.new("Pastel brown")
			rarm1.Name = "Right Arm"
			rarm1.Position = Vector3.new(2,4,0)

			FakeHead1 = new("Part",char.Torso.Gogeta1)
			FakeHead1.Size = Vector3.new(2, 1, 1)

			VFace1 = Instance.new("Decal",FakeHead1)

			FakeHead1.Name = "FakeHead1"
			FakeHead1.BrickColor = bc("Really black")
			FakeHead1.Material = "Neon"
			FakeHead1.Transparency = 1

			FakeHead16 = new("SpecialMesh",FakeHead1)
			FakeHead16.MeshType = "FileMesh"
			--aa10.Offset = Vector3.new(-0.225, 0.7, -0.1)
			FakeHead16.Offset = Vector3.new(-0, 0, -0)
			FakeHead16.Scale = v3(1.25, 1.25, 1.25)
			FakeHead16.MeshType = "Head"

			FakeHeada16 =new("Weld",FakeHead1)
			FakeHeada16.Part0 = head1
			FakeHeada16.Part1 = FakeHead1
			FakeHeada16.C0 = CFrame.Angles(0, math.rad(0), 0)

			VFace1.Texture = "rbxassetid://7017571741"

			ape55 = Instance.new("Part",char.Torso.Gogeta1)
			ape55.Transparency = 0

			weldape55 = Instance.new("Weld",ape55)
			weldape55.Part0 = torso1
			weldape55.Part1 = ape55

			ape55.BrickColor = bc("Bright red")

			weldape55.C0 = CFrame.new(0.4,-0.05,2)*CFrame.Angles(0,math.rad(90),math.rad(0))

			meshape55 = Instance.new("SpecialMesh",ape55)
			meshape55.MeshId = "rbxassetid://547956749"
			meshape55.Scale = v3(0.025,0.025,0.025)

			lleg1 = Instance.new("Part",char.Torso.Gogeta1)
			lleg1.Size = Vector3.new(1,2,1)
			lleg1.Name = "Left Leg"
			lleg1.Position = Vector3.new(1,2,0)

			rleg1 = Instance.new("Part",char.Torso.Gogeta1)
			rleg1.Size = Vector3.new(1,2,1)
			rleg1.Name = "Right Leg"
			rleg1.Position = Vector3.new(-1,2,0)

			headweld = Instance.new("Weld",char.Torso.Gogeta1.Head)
			headweld.Part0 = char.Torso.Gogeta1.Head
			headweld.Part1 = torso1
			headweld.C0 = CFrame.new(0, -1.5, 0)

			larmweld = Instance.new("Weld",char.Torso.Gogeta1.Torso)
			larmweld.Part0 = char.Torso.Gogeta1.Torso
			larmweld.Part1 = larm1
			larmweld.C0 = CFrame.new(1.5, 0, 0)

			rarmweld = Instance.new("Weld",char.Torso.Gogeta1.Torso)
			rarmweld.Part0 = char.Torso.Gogeta1.Torso
			rarmweld.Part1 = rarm1
			rarmweld.C0 = CFrame.new(-1.5, 0, 0)

			rlegweld = Instance.new("Weld",char.Torso.Gogeta1.Torso)
			rlegweld.Part0 = char.Torso.Gogeta1.Torso
			rlegweld.Part1 = rleg1
			rlegweld.C0 = CFrame.new(-0.5, -2, 0)

			llegweld = Instance.new("Weld",char.Torso.Gogeta1.Torso)
			llegweld.Part0 = char.Torso.Gogeta1.Torso
			llegweld.Part1 = lleg1
			llegweld.C0 = CFrame.new(0.5, -2, 0)

			modelhair1 = Instance.new("Model",char.Torso.Gogeta1)
			modelhair1.Name = "Hair"

			hair = Instance.new("Part",char.Torso.Gogeta1.Hair)
			hair.Name = "hair"
			hair.BrickColor = BrickColor.new("Bright red")
			hair.CanCollide = false

			hairmesh = Instance.new("SpecialMesh",hair)
			hairmesh.Scale = Vector3.new(6.4, 6.4, 6.4)
			hairmesh.Offset = Vector3.new(0, 0,0)
			hairmesh.MeshType = "FileMesh"
			hairmesh.MeshId = "rbxassetid://6586053249"

			hairweld = Instance.new("Weld",char.Torso.Gogeta1.Hair)
			hairweld.Part0 = char.Torso.Gogeta1.Head
			hairweld.Part1 = hair
			hairweld.C0 = CFrame.new(-0.1, 0, 0.61)

			rarm1.CFrame = char["Right Arm"].CFrame
			larm1.CFrame = char["Left Arm"].CFrame
			rleg1.CFrame = char["Right Leg"].CFrame
			lleg1.CFrame = char["Left Leg"].CFrame
			head1.CFrame = char["Head"].CFrame
			torso1.CFrame = char["Torso"].CFrame

			npctoplayer = Instance.new("Weld",char.HumanoidRootPart)
			npctoplayer.Part0 = char.HumanoidRootPart
			npctoplayer.Part1 = torso1
			npctoplayer.C0 = CFrame.new(-0, 0, -0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0))
			char.Torso.Gogeta1.Shirt.ShirtTemplate = "http://www.roblox.com/asset/?id=428344589"
			char.Torso.Gogeta1.Pants.PantsTemplate = "http://www.roblox.com/asset/?id=4814086789"
			newWeld(torso1, larm1, -1.5, 0.4, 0)
			larm1.Weld.C1 = CFrame.new(0, 0.5, 0)
			newWeld(torso1, rarm1, 1.5, 0.4, 0)
			rarm1.Weld.C1 = CFrame.new(0, 0.5, 0)
			newWeld(torso1, head1, 0, 1.5, 0)
			newWeld(torso1, lleg1, -0.5, -1, 0)
			lleg1.Weld.C1 = CFrame.new(0, 1, 0)
			newWeld(torso1, rleg1, 0.5, -1, 0)
			rleg1.Weld.C1 = CFrame.new(0, 1, 0)

			--------------------------------------
