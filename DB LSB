		Sbj5.Orientation = Sbj5.Orientation + Vector3.new(0,-3,0)
		VFace.Texture = "rbxassetid://4656549295"
		char.Shirt.ShirtTemplate = "http://www.roblox.com/asset/?id=4814086540"
