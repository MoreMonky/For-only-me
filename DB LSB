	Ball1.CanCollide = false
	Ball1.BrickColor = BrickColor.new("Institutional white")
	Ball1.Material = "Neon"
	Ball1.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,0,-26) * CFrame.Angles(-33,0,0)
	Ball1.Size = Vector3.new(39,2,39)
	local Ball2 = Instance.new("Part", Model)
	Ball2.Anchored = true
	Ball2.CanCollide = false
	Ball2.BrickColor = BrickColor.new("Cyan")
	Ball2.Material = "Neon"
	Ball2.CFrame = Ball1.CFrame
	Ball2.Size = Vector3.new(40,3,40)
	Ball2.Transparency = 0.5
	local MESH1 = Instance.new("CylinderMesh", Ball1)
	local MESH2 = Instance.new("CylinderMesh", Ball2)
	local ACTUALBALL3 = Instance.new("Part",Model)
	ACTUALBALL3.Anchored = true
	ACTUALBALL3.CanCollide = false
	ACTUALBALL3.BrickColor = BrickColor.new("Institutional white")
	ACTUALBALL3.Material = "Neon"
