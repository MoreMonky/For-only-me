end


function QuaternionFromCFrame(cf)
	local mx, my, mz, m00, m01, m02, m10, m11, m12, m20, m21, m22 = cf:components()
	local trace = m00 + m11 + m22
	if trace > 0 then
		local s = math.sqrt(1 + trace)
		local recip = 0.5 / s
		return (m21 - m12) * recip, (m02 - m20) * recip, (m10 - m01) * recip, s * 0.5
	else
		local i = 0
		if m00 < m11 then
			i = 1
		end
		if m22 > (i == 0 and m00 or m11) then
			i = 2
		end
		if i == 0 then
			local s = math.sqrt(m00 - m11 - m22 + 1)
			local recip = 0.5 / s
			return 0.5 * s, (m10 + m01) * recip, (m20 + m02) * recip, (m21 - m12) * recip
		elseif i == 1 then
			local s = math.sqrt(m11 - m22 - m00 + 1)
			local recip = 0.5 / s
			return (m01 + m10) * recip, 0.5 * s, (m21 + m12) * recip, (m02 - m20) * recip
		elseif i == 2 then
			local s = math.sqrt(m22 - m00 - m11 + 1)
			local recip = 0.5 / s
			return (m02 + m20) * recip, (m12 + m21) * recip, 0.5 * s, (m10 - m01) * recip
		end
	end
end
function clerp(a, b, t)
	local qa = {
		QuaternionFromCFrame(a)
	}
	local qb = {
		QuaternionFromCFrame(b)
	}
	local ax, ay, az = a.x, a.y, a.z
	local bx, by, bz = b.x, b.y, b.z
	local _t = 1 - t
	return QuaternionToCFrame(_t * ax + t * bx, _t * ay + t * by, _t * az + t * bz, QuaternionSlerp(qa, qb, t))
end
function QuaternionToCFrame(px, py, pz, x, y, z, w)
	local xs, ys, zs = x + x, y + y, z + z
	local wx, wy, wz = w * xs, w * ys, w * zs
	local xx = x * xs
	local xy = x * ys
	local xz = x * zs
	local yy = y * ys
	local yz = y * zs
	local zz = z * zs
	return CFrame.new(px, py, pz, 1 - (yy + zz), xy - wz, xz + wy, xy + wz, 1 - (xx + zz), yz - wx, xz - wy, yz + wx, 1 - (xx + yy))
end
function QuaternionSlerp(a, b, t)
	local cosTheta = a[1] * b[1] + a[2] * b[2] + a[3] * b[3] + a[4] * b[4]
	local startInterp, finishInterp
	if cosTheta >= 1.0E-4 then
		if 1 - cosTheta > 1.0E-4 then
			local theta = math.acos(cosTheta)
			local invSinTheta = 1 / Sin(theta)
			startInterp = Sin((1 - t) * theta) * invSinTheta
			finishInterp = Sin(t * theta) * invSinTheta
		else
			startInterp = 1 - t
			finishInterp = t
		end
	elseif 1 + cosTheta > 1.0E-4 then
		local theta = math.acos(-cosTheta)
		local invSinTheta = 1 / Sin(theta)
		startInterp = Sin((t - 1) * theta) * invSinTheta
		finishInterp = Sin(t * theta) * invSinTheta
	else
		startInterp = t - 1
		finishInterp = t
	end
	return a[1] * startInterp + b[1] * finishInterp, a[2] * startInterp + b[2] * finishInterp, a[3] * startInterp + b[3] * finishInterp, a[4] * startInterp + b[4] * finishInterp
end

Torsovelocity = root.Velocity * Vector3.new(1, 0, 1).magnitude
velocity = root.Velocity.y
charge = 1
change = 1
sine = 0
Anim = "Idle"
function rayCast(Position, Direction, Range, Ignore)
	return game:service("Workspace"):FindPartOnRay(Ray.new(Position, Direction.unit * (Range or 999.999)), Ignore)
end
function swait(num)
	if num == 0 or num == nil then
		game:service("RunService").Stepped:wait(0)
	else
		for i = 0, num do
			game:service("RunService").Stepped:wait(0)
		end
	end
end

game:GetService("RunService").Heartbeat:Connect(function()
	if MUITheme.SoundId == "rbxassetid://9043543323" and MUITheme.TimePosition > 15.216022864000024 then
		MUITheme.TimePosition = 0
	end

	Torsovelocity = root.Velocity * Vector3.new(1, 0, 1).magnitude
	velocity = root.Velocity.y
	sine = sine + change
	local hit, pos = rayCast(root.Position, CFrame.new(root.Position, root.Position - Vector3.new(0, 1, 0)).lookVector, 4, char)
	if equipped == true or equipped == false then

		if 1 < root.Velocity.y and hit == nil then
			Anim = "Jump"
			if attack == false and ice_mode == false then
				rootj.C0 = clerp(rootj.C0, RootCF * cn(0, 0, 0) * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-40), math.rad(0), math.rad(30)), 0.1)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.3, 0) * angles(math.rad(-40), math.rad(0), math.rad(-30)), 0.1)
				RH.C0 = clerp(RH.C0, cn(1, -0.9, -0.3) * RHCF * angles(math.rad(3), math.rad(0), math.rad(0)), 0.1)
				LH.C0 = clerp(LH.C0, cn(-1, -0.7, -0.5) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(0)), 0.1)
			end
		elseif root.Velocity.y < -1 and hit == nil and ice_mode == false then
			Anim = "Fall"
			if attack == false then
				rootj.C0 = clerp(rootj.C0, RootCF * cn(0, 0, 0) * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-20), math.rad(0), math.rad(50)), 0.1)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.3, 0) * angles(math.rad(-20), math.rad(0), math.rad(-50)), 0.1)
				RH.C0 = clerp(RH.C0, cn(1, -1, -0.3) * RHCF * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
				LH.C0 = clerp(LH.C0, cn(-1, -0.8, -0.3) * LHCF * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
			end
		elseif (tors.Velocity*Vector3.new(1, 0, 1)).magnitude < 2 and hit ~= nil and ice_mode == false then
			Anim = "Idle"
			if attack==false then

			end
		elseif (tors.Velocity*Vector3.new(1, 0, 1)).magnitude > 21 and hit ~= nil and ice_mode == false then
			Anim = "Run"
			if attack==false then
				rootj.C0 = clerp(rootj.C0, RootCF * cn(0, 0, -0.05 + .1 * math.cos(sine / 5)) * angles(math.rad(5), math.rad(0) + RootPart.RotVelocity.Y / 30, math.rad(5 * math.cos(sine / 10))), .2)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(-3), math.rad(0), math.rad(-5 * math.cos(sine / 10)) + RootPart.RotVelocity.Y / 15), .2)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-10 * math.cos(sine / 13)), math.rad(0), math.rad(4 * math.cos(sine / 13))+ RootPart.RotVelocity.Y / 15), .2)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(10 * math.cos(sine / 13)), math.rad(0), math.rad(4 * math.cos(sine / 13))+ RootPart.RotVelocity.Y / 15), .2)
				RH.C0 = clerp(RH.C0, cn(1, -1 + .1 * math.cos(sine / 2), 0) * RHCF * angles(math.rad(-2), math.rad(0), math.rad(30 * math.cos(sine / 7))+ RootPart.RotVelocity.Y / 15), .2)
				LH.C0 = clerp(LH.C0, cn(-1, -1 + .1 * math.cos(sine / 2), 0) * LHCF * angles(math.rad(-2), math.rad(0), math.rad(30 * math.cos(sine / 7))+ RootPart.RotVelocity.Y / 15), .2)
			end
		end
	end
	if equipped == true or equipped == false then

		if 1 < root.Velocity.Y and hit == nil then
			Anim = "Jump"
			if attack == false and ice_mode == true then
				rootj.C0 = clerp(rootj.C0, RootCF * cn(0, 0, 0) * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-40), math.rad(0), math.rad(30)), 0.1)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.3, 0) * angles(math.rad(-40), math.rad(0), math.rad(-30)), 0.1)
				RH.C0 = clerp(RH.C0, cn(1, -0.9, -0.3) * RHCF * angles(math.rad(3), math.rad(0), math.rad(0)), 0.1)
				LH.C0 = clerp(LH.C0, cn(-1, -0.7, -0.5) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(0)), 0.1)
			end
		elseif root.Velocity.Y < -1 and hit == nil and ice_mode == true then
			Anim = "Fall"
			if attack == false and ice_mode == true then
				rootj.C0 = clerp(rootj.C0, RootCF * cn(0, 0, 0) * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				tors.Neck.C0 = clerp(tors.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)
				RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(-20), math.rad(0), math.rad(50)), 0.1)
				LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.3, 0) * angles(math.rad(-20), math.rad(0), math.rad(-50)), 0.1)
				RH.C0 = clerp(RH.C0, cn(1, -1, -0.3) * RHCF * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
				LH.C0 = clerp(LH.C0, cn(-1, -0.8, -0.3) * LHCF * angles(math.rad(-5), math.rad(0), math.rad(0)), 0.1)
			end
		elseif(tors.Velocity*Vector3.new(1, 0, 1)).magnitude < 2 and hit ~= nil and ice_mode == true then
			Anim = "Idle"
			if attack == false and B_anim == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.Angles(-0.1, -0.1, -1), 0.2)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, -0.2 + v2980 / 8) * CFrame.Angles(-0, -0, 1), 0.2)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, 0.25, 0) * CFrame.Angles(1 - v2980 / 12, 0, 1), 0.2)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1, 0.5, -0.8) * CFrame.Angles(1.57, 0.5 + v2980 / 12, 0.8), 0.2)
				RH.C0 = RH.C0:Lerp(CFrame.new(0.8, -0.8 - v2980 / 8, 0.4) * CFrame.Angles(0, 1.6, 0) * CFrame.Angles(-0.4, -1.2, -0.2), 0.2)
				LH.C0 = LH.C0:Lerp(CFrame.new(-1, -0.8 - v2980 / 8, -0.5) * CFrame.Angles(0, -1.6, 0) * CFrame.Angles(-0.1, 0, -0), 0.2)
			elseif attack == false and B_animSSJ == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.new(0, 0, 0) * CFrame.Angles(-0.25, -0.25, 1.2), 0.3)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, -0.2 + v2980 / 8) * CFrame.Angles(0.5, -0.3, -1.2), 0.3)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, v2980 / 6, 0) * CFrame.Angles(2, 0, 0.4) * CFrame.Angles(0, 0.5, 0), 0.3)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, 0.25 + v2980 / 6, 0) * CFrame.Angles(1.2, 0, -1.2) * CFrame.Angles(0, -1, 0), 0.3)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(0, -0.5, 0.2), 0.3)
				LH.C0 = LH.C0:Lerp(CFrame.new(-0.7, -0.5 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.6, 1.3, 0) * CFrame.Angles(0.4, 0, 0), 0.3)
			elseif attack == false and B_animSSJ3 == true then
				change = 0.8

				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, -v2980 / 16, -0.4 + v2980 / 8) * CFrame.Angles(0.5, -0.3, -1.2), 0.3)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, v2980 / 6, 0) * CFrame.Angles(2, 0, 0.4) * CFrame.Angles(0, 0.5, 0), 0.3)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, 0.25 + v2980 / 6, 0) * CFrame.Angles(1.2, 0, -1.2) * CFrame.Angles(0, -1, 0), 0.3)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(0, -0.5, 0.2), 0.3)
				LH.C0 = LH.C0:Lerp(CFrame.new(-0.7, -0.5 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.6, 1.3, 0) * CFrame.Angles(0.4, 0, 0), 0.3)
			elseif attack == false and B_animSSJG == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.Angles(0.1, 0, 0), 0.2)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, 0-.05*math.sin(tick()*2) + v2980 / 8) * CFrame.Angles(-0, 0, 0), 0.2)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, 0.45, -0) * CFrame.Angles(0 - v2980 / 12, -0.2, 0.1 + v2980 / 12), 0.2)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, 0.45, -0) * CFrame.Angles(0 + v2980 / 12, 0.2, -0.1 - v2980 / 12), 0.2)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 + v2980/ 8, 0+.05*math.sin(tick()*2)) * CFrame.Angles(0, 1.6, 0) * CFrame.Angles(-0.1, -0.25, 0), 0.2)
				LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 + v2980/ 8, -0+.05*math.sin(tick()*2)) * CFrame.Angles(0, -1.6, 0) * CFrame.Angles(-0.1, 0.5, -0), 0.2)
			elseif attack == false and B_animSSJB == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.new(0, 0, 0) * CFrame.Angles(0.05, -0.025, -1.157), 0.2)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, 0 + v2980 / 8) * CFrame.Angles(0.05, 0.05, 1.3), 0.2)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.4, 0, 0.1) * CFrame.Angles(2, 0, 0.8) * CFrame.Angles(0, -1, 0), 0.2)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, -0.2, 0) * CFrame.Angles(2, 0, 0) * CFrame.Angles(0, 1, 0), 0.2)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(-0.08, -0.5, 0.1), 0.2)


				LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.08, 0.3, -0.05), 0.2)
			elseif attack == false and B_animSSJBKK == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.Angles(0.1, 0, 0), 0.2)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, 0 + v2980 / 8) * CFrame.Angles(-0, 0, 0), 0.2)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, 0.45, -0) * CFrame.Angles(0 - v2980 / 12, -0.2, 0.1 + v2980 / 12), 0.2)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, 0.45, -0) * CFrame.Angles(0 + v2980 / 12, 0.2, -0.1 - v2980 / 12), 0.2)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.6, 0) * CFrame.Angles(-0.1, -0.25, 0), 0.2)
				LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 - v2980 / 8, -0) * CFrame.Angles(0, -1.6, 0) * CFrame.Angles(-0.1, 0.5, -0), 0.2)

			elseif attack == false and UltMode == true then
				change = 0.8
				RW.C0 = clerp(RW.C0, CF(1.5 -0.30, 1, 0) * ANGLES(RAD(60), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
				LW.C0 = clerp(LW.C0, CF(-1.5 +0.30, 1, 0) * ANGLES(RAD(60), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 2 / Animation_Speed)
				lerpz(RJ, "C0", RJC0 * cf(0, 0, -0.35) * ang(rd(10), rd(0), rd(0)), 0.5)
				lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(35), rd(0), rd(0)), 0.5)
				lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)
				lerpz(RS, "C1", RSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
				lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)
				lerpz(LS, "C1", LSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)

				lerpz(RH, "C0", RHC0 * cf(0.35, 0.35, 0.05) * ang(rd(-10), rd(-10), rd(10)), 0.5)
				lerpz(RH, "C1", RHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
				lerpz(LH, "C0", LHC0 * cf(-0.35, 0.35, 0.05) * ang(rd(-10), rd(10), rd(-10)), 0.5)
				lerpz(LH, "C1", LHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)

			elseif attack == false and B_animGB == true then
				change = 0.8

				tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.Angles(0.1, 0, 0), 0.2)
				rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, 0 + v2980 / 8) * CFrame.Angles(-0, 0, 0), 0.2)
				RW.C0 = RW.C0:Lerp(CFrame.new(1.5, 0.45, -0) * CFrame.Angles(90 - v2980 / 12, -0, 90 + v2980 / 12), 0.2)
				LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, 0.45, -0) * CFrame.Angles(0 + v2980 / 12, 0.2, -0.1 - v2980 / 12), 0.2)
				RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.6, 0) * CFrame.Angles(-0.1, -0.25, 0), 0.2)
				LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 - v2980 / 8, -0) * CFrame.Angles(0, -1.6, 0) * CFrame.Angles(-0.1, 0.5, -0), 0.2)
			end

		elseif (tors.Velocity*Vector3.new(1, 0, 1)).magnitude < 2000 and hit ~= nil and ice_mode == true then
			Anim = "Walk"
			if attack == false then
				rootj.C0 = clerp(rootj.C0, RootCF * CF(0* Player_Size, 0* Player_Size, -0.175 + 0.025 * Cos(sine / 3.5) + -Sin(sine / 3.5) / 7* Player_Size) * angles(Rad(3 - 2.5 * Cos(sine / 3.5)), Rad(0) - root.RotVelocity.Y / 75, Rad(8 * Cos(sine / 7))), 0.15)
				tors.Neck.C0 = clerp(tors.Neck.C0, necko* CF(0, 0, 0 + ((1* Player_Size) - 1)) * angles(Rad(-1), Rad(0), Rad(0) - hed.RotVelocity.Y / 15), 0.15)
				RH.C0 = clerp(RH.C0, CF(1* Player_Size, -0.8 - 0.5 * Cos(sine / 7) / 2* Player_Size, 0.6 * Cos(sine / 7) / 2* Player_Size)  * angles(Rad(-15 - 15 * Cos(sine / 7)) - rl.RotVelocity.Y / 75 + -Sin(sine / 7) / 2.5, Rad(90 - 10 * Cos(sine / 7)), Rad(0)) * angles(Rad(0 + 2 * Cos(sine / 7)), Rad(0), Rad(0)), 0.3)
				LH.C0 = clerp(LH.C0, CF(-1* Player_Size, -0.8 + 0.5 * Cos(sine / 7) / 2* Player_Size, -0.6 * Cos(sine / 7) / 2* Player_Size) * angles(Rad(-15 + 15 * Cos(sine / 7)) + ll.RotVelocity.Y / 75 + Sin(sine / 7) / 2.5, Rad(-90 - 10 * Cos(sine / 7)), Rad(0)) * angles(Rad(0 - 2 * Cos(sine / 7)), Rad(0), Rad(0)), 0.3)
				RW.C0 = clerp(RW.C0, CF(1.5* Player_Size, 0.5 + 0.05 * Sin(sine / 7)* Player_Size, 0* Player_Size) * angles(Rad(56)  * Cos(sine / 7) , Rad(10 * Cos(sine / 7)), Rad(6) - ra.RotVelocity.Y / 75), 0.1)
				LW.C0 = clerp(LW.C0, CF(-1.5* Player_Size, 0.5 + 0.05 * Sin(sine / 7)* Player_Size, 0* Player_Size) * angles(Rad(-56)  * Cos(sine / 7) , Rad(10 * Cos(sine / 7)) ,	Rad(-6) + la.RotVelocity.Y / 75), 0.1)

			end

		end
	end
end)

PlayerGui = Player.PlayerGui

function CreateTextGui(Parent, Text, TextSpeed, TextColor, TextStrokeColor, Font)
	local ToWrite = Text
	local DoStroke = false
	local Billboard = Instance.new("BillboardGui")
	Billboard.Adornee = Parent
	Billboard.Name = tostring(tick()^20*5*math.random())
	Billboard.Size = UDim2.new(2, 0, 1.2, 0)
	Billboard.StudsOffset = Vector3.new(-6, 0, 0)
	Billboard.Parent = Parent
	local WriterGui = Instance.new("TextBox")
	WriterGui.Size = UDim2.new(7, 0, 0.5, 0)
	WriterGui.TextScaled = true
	WriterGui.FontSize = "Size24"
	WriterGui.TextSize = 22
	WriterGui.BackgroundTransparency = 1
	WriterGui.TextTransparency = 1
	WriterGui.TextStrokeTransparency = 1
	if TextStrokeColor ~= "None" then
		DoStroke = true
		WriterGui.TextStrokeColor3 = TextStrokeColor
	end
	WriterGui.Font = Font
	WriterGui.Name = tostring(tick()^20*5*math.random())
	WriterGui.Text = ""
	WriterGui.TextColor3 = TextColor
	WriterGui.Parent = Billboard
	coroutine.resume(coroutine.create(function()
		for i = 1, 20 do
			WriterGui.TextTransparency = WriterGui.TextTransparency - 0.05
			if DoStroke == true then
				WriterGui.TextStrokeTransparency = WriterGui.TextTransparency
			end
			Billboard.StudsOffset = Billboard.StudsOffset + Vector3.new(0, 0.15, 0)
			game:GetService("RunService").Heartbeat:Wait(25)
		end
	end))

	coroutine.resume(coroutine.create(function()
		for i = 1, #ToWrite do
			WriterGui.Text = string.sub(ToWrite, 1, i)
			wait(0.4/(TextSpeed*2.5))
		end
		wait(1.25)
		for i = 1, 20 do
			WriterGui.TextTransparency = WriterGui.TextTransparency + 0.05
			if DoStroke == true then
				WriterGui.TextStrokeTransparency = WriterGui.TextTransparency
			end
			Billboard.StudsOffset = Billboard.StudsOffset + Vector3.new(0, 0.15, 0)
			game:GetService("RunService").Heartbeat:Wait(25)
		end
		Billboard:Destroy()
	end))
end

function CreateSound(ID, Parent, Volume, Pitch, TimePos, Loop, StayForever, CustomName)
	local New = nil
	coroutine.resume(coroutine.create(function()
		New = Instance.new("Sound")
		New.Name = CustomName
		New.Parent = Parent
		New.Volume = Volume
		New.SoundId = "rbxassetid://"..ID
		New.Pitch = Pitch
		New.TimePosition = TimePos
		New.Looped = Loop
		if StayForever == false then
			New.Ended:Connect(function()
				New:Destroy()
			end)
		end
		New:Play()
	end))
	return New
end



