		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(-180),math.rad(0),math.rad(-10)), 0.1)--arm

		lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
		lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)

	end

	if transforming2 == true then
		wait()

		tors.Neck.C0 = lerpz(tors.Neck.C0, CFrame.new(0,1.5,0)*CFrame.Angles(math.rad(20),math.rad(0),math.rad(0)), 0.1)--head
		rootj.C0 = lerpz(rootj.C0, CFrame.new(0, -1, 0) * CFrame.Angles(math.rad(10), math.rad(0), math.rad(0)), 0.1)--torso
		RW.C0 = lerpz(RW.C0, CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(-30),math.rad(0),math.rad(15)), 0.1)--arm
		LW.Weld.C0 = lerpz(LW.Weld.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(-30),math.rad(0),math.rad(-15)), 0.1)--arm
		LH.C0 = lerpz(LH.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(-20), math.rad(0), math.rad(-10)), 0.1)--leg
		RH.C0 = lerpz(RH.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(-20), math.rad(0), math.rad(10)), 0.1)--leg

	end

	if beam3 == true then
		beamgo4 = beamgo4 + 1
		beamgo3 = beamgo3 + 0.018
		a1s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) + torso.CFrame.lookVector * beamgo4
		aa1s.Scale = v3(2,math.random(2,2.4),math.random(2,2.4))
		a3s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) + torso.CFrame.lookVector * beamgo4 * 5
		a1s.Size = a1s.Size + v3(beamgo3,0,0)
		aa2s.Scale = v3(200,math.random(150,170),math.random(150,170))
		aa3s.Scale = v3(250,math.random(150,170),math.random(150,170))
		a2s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) - torso.CFrame.lookVector * 1
	end

	if beam4 == true then
		a1s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) + torso.CFrame.lookVector * beamgo4
		a2s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) + torso.CFrame.lookVector * 8
		a3s.CFrame = torso.CFrame * CFrame.Angles(0,rad(90),0) + torso.CFrame.lookVector * beamgo4 * 1.93
	end

end)



function TailDisAppear()
	Carrot = false
	for i,v in pairs(char:GetChildren()) do

		if v.Name == "TailPart" and v.Transparency == 0 then

			v.Transparency = 1

		end

	end

end

function TailAppear()
	Carrot = true
	for i,v in pairs(char:GetChildren()) do

		if v.Name == "TailPart" and v.Transparency == 1 then
			localValue = false
			v.Transparency = 0

		end

	end

end

TailDisAppear()

