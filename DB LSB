	if Charging == true then

		Beam1.Width0 = Beam1.Width0 + 1
		Beam1.Width1 = Beam1.Width1 + 1

		Beam2.Width0 = Beam2.Width0 + 1
		Beam2.Width1 = Beam2.Width1 + 1


		if OnePiece == 0 and Gauge.Image ~= "rbxassetid://11531039832" then
			Gauge.Image = "rbxassetid://11531039832"
		end


		if OnePiece == 560 and EndOfTheRun == true then

			BaseGauge.Image = "http://www.roblox.com/asset/?id=11531035810"
			BillboardGaugeImage2.Image = "rbxassetid://11531035810"
			Gauge.Image = "http://www.roblox.com/asset/?id=11531035810"

			BillboardGaugeImage.Image = Gauge.Image
			BillboardGaugeImage.ImageRectSize = Gauge.ImageRectSize
			BillboardGaugeImage.ImageRectOffset = Gauge.ImageRectOffset

		end

		if EndOfTheRun == false then

			r:UpdateLabel(tick() % 10 / 10)

			BillboardGaugeImage.Image = Gauge.Image
			BillboardGaugeImage.ImageRectSize = Gauge.ImageRectSize
			BillboardGaugeImage.ImageRectOffset = Gauge.ImageRectOffset

