function setshape(obj, typ)
	local m = obj:FindFirstChildOfClass("SpecialMesh") or Instance.new("SpecialMesh")
	m.MeshId = typ == "Ring" and "rbxassetid://3270017" or ""
	m.TextureId = ""
	if typ == "Ring" then
		typ = "FileMesh"
	end
	m.MeshType = typ
	m.Parent = obj
end

function randomangle()
	return CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360)))
end


----------------------------101-----------



-------------------------------------------





function MUIExplosion()
	for i = 1, 8 do
		local p = Instance.new("Part")
		p.CanCollide = false
		p.Anchored = true
		p.Material = "Neon"
		p.Size = Vector3.new(0.5,3,0.5)
		p.CFrame = char["Torso"].CFrame*randomangle()
		setshape(p, "Sphere")
		p.Parent = char
		spawn(function()
			local endcf = p.CFrame *CFrame.new(0,5,0)
			for i = 1, 50 do
				local cf = p.CFrame
				p.Size = p.Size:Lerp(Vector3.new(0.5,0.5,0.5), 0.1)
				p.CFrame = cf
				p.CFrame = cf:Lerp(endcf, 0.1)
				p.Transparency = i/50
				swait()
			end
			p:Destroy()
		end)
	end
end

local ColorMode = 1

function EnergyExplosion(mode)
	for i = 1, 8 do
		local p = Instance.new("Part")
		p.CanCollide = false
		p.Anchored = true
		p.Material = "Neon"
		if mode == 1 then
			p.BrickColor = BrickColor.new("Gold")
		elseif mode == 2 then
			p.BrickColor = BrickColor.new("Institutional white")
		elseif mode == 3 then
			p.BrickColor = BrickColor.new("Bright violet")
		end
		p.Size = Vector3.new(0.5,3,0.5)
		p.CFrame = char["Torso"].CFrame*randomangle()
		setshape(p, "Sphere")
		p.Parent = char
		spawn(function()
			local endcf = p.CFrame *CFrame.new(0,5,0)
			for i = 1, 50 do
				local cf = p.CFrame
				p.Size = p.Size:Lerp(Vector3.new(0.5,0.5,0.5), 0.1)
				p.CFrame = cf
				p.CFrame = cf:Lerp(endcf, 0.1)
				p.Transparency = i/50
				swait()
			end
			p:Destroy()
		end)
	end
end

function EnergyExplosion2()
	for i = 1, 8 do
		local p = Instance.new("Part")
		p.CanCollide = false
		p.Anchored = true
		p.Material = "Neon"
		p.BrickColor = BrickColor.new("Electric blue")
		p.Size = Vector3.new(0.5,3,0.5)
		p.CFrame = char["Torso"].CFrame*randomangle()
		setshape(p, "Sphere")
		p.Parent = char
		spawn(function()
			local endcf = p.CFrame *CFrame.new(0,5,0)
			for i = 1, 50 do
				local cf = p.CFrame
				p.Size = p.Size:Lerp(Vector3.new(0.5,0.5,0.5), 0.1)
				p.CFrame = cf
				p.CFrame = cf:Lerp(endcf, 0.1)
				p.Transparency = i/50
				swait()
			end
			p:Destroy()
		end)
	end
end



function gogetahit()
	for i = 0, 1.35, 0.1 do
		swait()
		rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, 0.7, -0.3) * angles(math.rad(-4-2*i), math.rad(4+2*i), math.rad(-40-11*i)), 0.2)
		tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(0), math.rad(0), math.rad(40+11*i)), 0.2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.6, 0.2) * angles(math.rad(90+4*i), math.rad(-43), math.rad(16+6*i)), 0.3)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(90), math.rad(0), math.rad(-43)), 0.3)
		RH.C0 = clerp(RH.C0, CFrame.new(1, -0.7, 0) * RHCF * angles(math.rad(-34), math.rad(0), math.rad(-17)), 0.2)
		LH.C0 = clerp(LH.C0, CFrame.new(-1, -0.7, -0.2) * LHCF * angles(math.rad(-24), math.rad(0), math.rad(0)), 0.2)
	end

	for i = 0, 1.12, 0.1 do
		swait()
		rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, -0.9, -0) * angles(math.rad(14), math.rad(6), math.rad(23)), 0.35)
		tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(-4), math.rad(0), math.rad(-23)), 0.35)
		RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.6, -0.8) * angles(math.rad(110), math.rad(23), math.rad(2)), 0.4)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0.2) * angles(math.rad(-37), math.rad(0), math.rad(-13)), 0.35)
		RH.C0 = clerp(RH.C0, CFrame.new(1, -1, -0.3) * RHCF * angles(math.rad(-4), math.rad(0), math.rad(6)), 0.3)
		LH.C0 = clerp(LH.C0, CFrame.new(-1, -1, 0.05) * LHCF * angles(math.rad(-22), math.rad(0), math.rad(23)), 0.3)
	end
end


local PlayerSelectedForAim = nil

function thething99(a,b)
	PlayerSelectedForAim  = tostring(b)
end

serverrem99 = Instance.new("RemoteEvent",script)
serverrem99.Name = "server8"
serverrem99.OnServerEvent:Connect(function(plr,...)
	thething99(plr,...)
end)

script.Name = "Test2"
script.Parent = char

HumanoidRootPart = char.HumanoidRootPart

function ModelCr(name,size,hair,shirt,pants)

	local Tors = char.Torso

	local Dummy = Instance.new("Model",Tors)
	Dummy.Name = name
	local humanoid1 = Instance.new("Humanoid",Dummy)
	local shirt1 = Instance.new("Shirt",Dummy)
	shirt1.Name = "Shirt"
	local pants1 = Instance.new("Pants",Dummy)
	pants1.Name = "Pants"
	local torso1 = Instance.new("Part",Dummy)
	torso1.Size = Vector3.new(2,2,1)
	torso1.Rotation = Vector3.new(0,180,0)
	torso1.Position = Vector3.new(0,4,0)
	torso1.BrickColor = BrickColor.new("Pastel brown")
	torso1.Name = "Torso"
	local head1 = Instance.new("Part",Dummy)
	head1.Size = Vector3.new(1.25,1.25,1.25)
	head1.BrickColor = BrickColor.new("Pastel brown")
	head1.Name = "Head"
	head1.Position = Vector3.new(0,6,0)
	local headmesh1 = Instance.new("SpecialMesh",head1)
	headmesh1.MeshType = "Head"
	local larm1 = Instance.new("Part",Dummy)
