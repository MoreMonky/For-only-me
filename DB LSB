local DodgeMode = false

Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "2" and UltraInstinct == true and DodgeMode == false then

		DodgeMode = true
		instinct = true
	elseif key == "2" and UltraInstinct == true and DodgeMode == true then
		DodgeMode = false
		instinct = false
	end
end)

hum.BreakJointsOnDeath = false
root.Touched:connect(function(hit)--dodge
	local h = hit.Parent:FindFirstChild("Humanoid")
	local h2 = hit.Parent:FindFirstChild("Torso")
	if h ~= nil then
		if hum.Health > 0 and instinct == true then
			attack = true
			hum.MaxHealth = math.huge
			wait(0.01)
			hum.Health = math.huge
			local ay = math.random(1,2)
			if ay == 1 then
				rootj.C0 = rootj.C0 + v3(3.4,0,0)


			elseif ay == 2 then
				rootj.C0 = rootj.C0 + v3(-3,4,0,0)


			end

		end
	end
	attack = false
end)


Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "l" then
		if Base == true then
			if Base == true and canattack == true and PowerDown == true then

				canattack = false
				CanFight = false
				canwalk = false
				hum.WalkSpeed = 0
				a8b.Transparency = 0
