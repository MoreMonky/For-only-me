	end	

	function SSJTF()

		for i = 1, 50 do
			swait()
			RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.25,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-15)), 0.1)--arm
			LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.25,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(15)), 0.1)--arm
			lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(27), rd(0), rd(0)), 0.5)
			lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)
			lerpz(RS, "C1", RSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)
			lerpz(LS, "C1", LSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)
			lerpz(RH, "C1", RHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
			lerpz(LH, "C1", LHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)

		end

	end

	function SSJ5TF()
		for i = 1,75 do
			wait()
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(25), rd(0), rd(0)), 0.5)
			rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, 0 + v2980 / 8) * CFrame.Angles(-0, 0, 0), 0.2)
			RW.C0 = RW.C0:Lerp(CFrame.new(1.5, -0.2, 0.1) * CFrame.Angles(2, 0, 0.8) * CFrame.Angles(0, -1, 0), 0.2)
			LW.C0 = LW.C0:Lerp(CFrame.new(-1.5, -0.2, 0.1) * CFrame.Angles(2, 0, -0.8) * CFrame.Angles(0, 1, 0), 0.2)
			RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(-0.08, -0.5, 0.1), 0.2)
			LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.08, 0.3, -0.05), 0.2)
		end
	end

	if transforming1 == true then
		for i = 1,150 do
			wait()	
			RW.C0 = clerp(RW.C0, CF(1.5 -0.30, 1, 0) * ANGLES(RAD(90), RAD(0), RAD(0)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
			LW.C0 = clerp(LW.C0, CF(-1.5 +0.30, 1, 0) * ANGLES(RAD(90), RAD(0), RAD(0)) * LEFTSHOULDERC0, 2 / Animation_Speed)
			lerpz(RJ, "C0", RJC0 * cf(0, 0, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)
			lerpz(RS, "C1", RSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)
			lerpz(LS, "C1", LSC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(RH, "C0", RHC0 * cf(0.35, 0.35, 0.05) * ang(rd(-10), rd(-10), rd(10)), 0.5)
			lerpz(RH, "C1", RHC1 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(LH, "C0", LHC0 * cf(-0.35, 0.35, 0.05) * ang(rd(-10), rd(10), rd(-10)), 0.5)
