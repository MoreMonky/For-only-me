		local Ball1 = Instance.new("Part", Model)
		Ball1.Anchored = true
		Ball1.CanCollide = false
		Ball1.BrickColor = BrickColor.new("Cyan")
		Ball1.Material = "Neon"
		Ball1.Name = "Ball1"
		Ball1.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,0,-8) * CFrame.Angles(-33,0,0)
