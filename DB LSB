	ACTUALBALL4.Size = Vector3.new(42,42,42)
	ACTUALBALL4.Shape = "Ball"
	ACTUALBALL4.Transparency = 0.5

	Ball1.Touched:Connect(function(hit)
		if hit.Parent ~= char then
			for i, v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "Humanoid" then
					v.Health = v.Health - (v.MaxHealth/200)

				end
			end
		end
	end)
	ACTUALBALL.Touched:Connect(function(hit)
		if hit.Parent ~= char then
			for i, v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "Humanoid" then
					v.Health = v.Health - (v.MaxHealth/200)
				end
			end
		end
	end)
	ACTUALBALL3.Touched:Connect(function(hit)
		if hit.Parent ~= char then
			for i, v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "Humanoid" then

					v.Health = v.Health - (v.MaxHealth/200)

				end
			end
		end
	end)

	for i = 1, 60 do
		wait()

		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(95),math.rad(-10),math.rad(-30)), 0.5)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(75),math.rad(10),math.rad(30)), 0.5)--arm
		lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
		lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
		Ball1.Size = Ball1.Size + Vector3.new(0,8,0)
		Ball2.Size = Ball2.Size + Vector3.new(0,8,0)
		Ball1.CFrame = Ball1.CFrame * CFrame.new(0,4,0)
		Ball2.CFrame = Ball1.CFrame
		ACTUALBALL3.CFrame = ACTUALBALL3.CFrame * CFrame.new(0,8,0)
		ACTUALBALL4.CFrame = ACTUALBALL3.CFrame

	end

	CreateSound(5911250491, Head, 1, 1, 0, false, false, "Goku")
	CreateSound(4665878561, Head, 1, 1, 0, false, false, "Beam")
	
	local Model2 = Instance.new("Model", char)
	Model2.Name = "Blast"
	local ACTUALBALL90 = Instance.new("Part",Model2)
	ACTUALBALL90.Anchored = true
	ACTUALBALL90.CanCollide = false
	ACTUALBALL90.BrickColor = BrickColor.new("Institutional white")
	ACTUALBALL90.Material = "Neon"
	ACTUALBALL90.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,0,-29)
	ACTUALBALL90.Size = Vector3.new(61,61,61)
	ACTUALBALL90.Shape = "Ball"
	local ACTUALBALL902 = Instance.new("Part",Model2)
	ACTUALBALL902.Anchored = true
	ACTUALBALL902.CanCollide = false
