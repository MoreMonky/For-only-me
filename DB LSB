				Cycled = true

			elseif Gauge.Image ~= "rbxassetid://11531039832" and Cycled == true then
				Cycled = false
				OnePiece = 0

			else	
				OnePiece = OnePiece + 1

				if OnePiece == 560 then
					EndOfTheRun = true
				end	
			end

		end
	end


