	Dummy.Shirt.ShirtTemplate = "http://www.roblox.com/asset/?id=268667762"
	Dummy.Pants.PantsTemplate = "http://www.roblox.com/asset/?id=268667843"
	newWeld(torso1, larm1, -1.5, 0.4, 0)
	larm1.Weld.C1 = CFrame.new(0, 0.5, 0)
