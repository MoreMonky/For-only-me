				for i, v in pairs(hit.Parent:GetChildren()) do
					if v.Name == "Humanoid" then
						v.Health = v.Health - (v.MaxHealth/300)
					end
				end
			end

		end) 
		ACTUALBALL3.Touched:Connect(function(hit)

			if hit.Parent ~= char then
				for i, v in pairs(hit.Parent:GetChildren()) do
					if v.Name == "Humanoid" then
						v.Health = v.Health - (v.MaxHealth/300)
					end
				end
			end

		end)
		for i = 1, 60 do
			wait()

			RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(95),math.rad(-10),math.rad(-30)), 0.5)--arm
			LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(75),math.rad(10),math.rad(30)), 0.5)--arm
			lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
			lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
			lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
			lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
			lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
			Ball1.Size = Ball1.Size + Vector3.new(0,8,0)
			Ball2.Size = Ball2.Size + Vector3.new(0,8,0)
			Ball1.CFrame = Ball1.CFrame * CFrame.new(0,4,0)
			Ball2.CFrame = Ball1.CFrame
			ACTUALBALL3.CFrame = ACTUALBALL3.CFrame * CFrame.new(0,8,0)
			ACTUALBALL4.CFrame = ACTUALBALL3.CFrame

		end


		S5:Destroy()
		S6:Destroy()
		S7:Destroy()
		S8:Destroy()
		for i = 1, 50 do
			wait()
			Ball1.Transparency = Ball1.Transparency + 0.02
			Ball2.Transparency = Ball2.Transparency + 0.02
			ACTUALBALL.Transparency = ACTUALBALL.Transparency + 0.02
			ACTUALBALL2.Transparency = ACTUALBALL2.Transparency + 0.02
			ACTUALBALL3.Transparency = ACTUALBALL3.Transparency + 0.02
			ACTUALBALL4.Transparency = ACTUALBALL4.Transparency + 0.02
		end
		Model:Destroy()
		HumanoidRootPart.Anchored = false

	end
end


function FusionDance(model)	
	
	local head1 = model.Head
	local torso1 = model.Torso
	local rarm1 = model["Right Arm"]
	local larm1 = model["Left Arm"]
	local lleg1 = model["Left Leg"]
	local rleg1 = model["Right Leg"]
	
	local npctoplayer = char.Torso[model.Name.."Weld"]
	
	B_anim = false
	RW.C0 = Lerp2(RW.C0, CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)), 1)--arm
	LW.C0 = Lerp2(LW.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)), 1)--arm
	lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
	lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
	lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
	lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
	lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
	lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)

	for i = 1, 20 do
		wait()
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-90)), 0.1)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-90)), 0.1)--arm
		lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
		lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
		head1.Weld.C0 = Lerp2(head1.Weld.C0, CFrame.new(0,1.5,0)*CFrame.Angles(math.rad(-0),math.rad(0),math.rad(0)), 0.1)--head
