		local	CR2 = CFrame.new(Welds[7].C1:toEulerAnglesXYZ()).Position
		local	CR3 = CFrame.new(Vector3.new(math.pi / 9 + math.random(-5, 5) / 20, -math.pi / 12, 0)).Position
		local	AddTo1 = (CR3 - CR2) / 20
		for a = 1, 10 do
			wait()
