	rarm1.Weld.C1 = CFrame.new(0, 0.5, 0)
	newWeld(torso1, head1, 0, 1.5, 0)
	newWeld(torso1, lleg1, -0.5, -1, 0)
	lleg1.Weld.C1 = CFrame.new(0, 1, 0)
	newWeld(torso1, rleg1, 0.5, -1, 0)
	rleg1.Weld.C1 = CFrame.new(0, 1, 0)

	return Dummy

end	

function UltraKamehameha()
	aaaae.Enabled = true
	char.HumanoidRootPart.Anchored = true
	local Xd = Instance.new("Sound",char.HumanoidRootPart)
	Xd.Volume = 1
	Xd.SoundId = "rbxassetid://3177518324"
	Xd:Play()
	wait(1)
	Xd:Stop()
	Xd:Destroy()

	for i=0,5 do
		char.HumanoidRootPart.Position = char.HumanoidRootPart.Position + Vector3.new(0,1.5,0)
		wait()
	end

	aaaae.Enabled = false
	char.HumanoidRootPart.Anchored = true
	local sound1 = Instance.new("Sound", char.HumanoidRootPart)
	sound1.SoundId = "rbxassetid://2889442449"
	sound1.Volume = 0.5
	sound1:Play()
	local sound2 = Instance.new("Sound", char.HumanoidRootPart)
	sound2.SoundId = "rbxassetid://2889601147"
	sound2.Volume = 1
	sound2:Play()
	local Part = Instance.new("Part", char)
	Part.Anchored = false
	Part.CanCollide = false
	Part.BrickColor = BrickColor.new("Institutional white")
	Part.Material = "Neon"
	Part.CastShadow = false
	Part.Size = Vector3.new(1.3,1.3,1.3)
	Part.Shape = "Ball"
	local Part199 = Instance.new("Part", Part)
	Part199.Anchored = false
	Part199.CanCollide = false
	Part199.BrickColor = BrickColor.new("Cyan")
	Part199.Material = "Neon"
	Part199.CastShadow = false
	Part199.Size = Vector3.new(2,2,2)
	Part199.Transparency = 0.5
	Part199.Shape = "Ball"
	local weld = Instance.new("Weld", Part)
	weld.Part0 = char["Right Arm"]
	weld.Part1 = Part
	weld.C0 = CFrame.new(-0.35, -1, 0.35)
	local weld2 = Instance.new("Weld", Part)
	weld2.Part0 = Part
	weld2.Part1 = Part199
	weld2.C0 = CFrame.new(0,0,0)

	for i = 1, 125 do
		wait()
		char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame:Lerp(CFrame.new(char.HumanoidRootPart.Position, CurrentPos),0.5)
		rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, -0.2 + v2980 / 8) * CFrame.Angles(0.5, -0.3, -1.2), 0.3)
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(85),math.rad(-10),math.rad(-30)), 1)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(85),math.rad(10),math.rad(30)), 1)--arm
		RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(0, -0.5, 0.2), 0.3)
		LH.C0 = LH.C0:Lerp(CFrame.new(-0.7, -0.5 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.6, 1.3, 0) * CFrame.Angles(0.4, 0, 0), 0.3)
		tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.new(0, 0, 0) * CFrame.Angles(-0.25, -0.25, 1.2), 0.3)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	

	end

	--wait(3)
	Part:Destroy()
	sound2:Destroy()
	local sound3 = Instance.new("Sound", char.HumanoidRootPart)
	sound3.SoundId = "rbxassetid://2889442962"
	sound3.Volume = 0.5
	sound3:Play()
	local sound4 = Instance.new("Sound", char.HumanoidRootPart)
	sound4.SoundId = "rbxassetid://2889601935"
	sound4.Volume = 1
	sound4:Play()
	--2889601935
	local Model = Instance.new("Model", char)
	Model.Name = "Blast"
	local ACTUALBALL = Instance.new("Part",Model)
	ACTUALBALL.Anchored = true
	ACTUALBALL.CanCollide = false
	ACTUALBALL.BrickColor = BrickColor.new("Institutional white")
	ACTUALBALL.Material = "Neon"
	ACTUALBALL.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,0,-23)
	ACTUALBALL.Size = Vector3.new(41,41,41)
