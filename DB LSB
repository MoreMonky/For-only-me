	MUITheme:Play()
	for i = 1, 100 do
		wait()

		--	rootj.C0 = Lerp(rootj.C0, CFrame.new(0,1.5,0)*CFrame.Angles(math.rad(0),math.rad(70),math.rad(0)), 0.1)--head
