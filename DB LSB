		rarm1.Weld.C0 = Lerp2(rarm1.Weld.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(90)), 0.1)--arm
		larm1.Weld.C0 = Lerp2(larm1.Weld.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(90)), 0.1)--arm
		lleg1.Weld.C0 = Lerp2(lleg1.Weld.C0, CFrame.new(-0.5, -1, 0) * CFrame.Angles(math.rad(-0), math.rad(0), math.rad(-3)), 0.1)--leg
		rleg1.Weld.C0 = Lerp2(rleg1.Weld.C0, CFrame.new(0.5, -1, 0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(3)), 0.1)--leg
	end
	wait(0.5)
	for i = 1, 20 do
		wait()
		npctoplayer.C0 = CFrame.new(-5.3, 0, -0) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0))
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(0),math.rad(-0),math.rad(90)), 0.1)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(90)), 0.1)--arm
