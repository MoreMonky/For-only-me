	SSbj4.Scale = v3(-4, 4, 4)
	SSbj4.MeshId = "rbxassetid://6494549693"

	local	Sbj5 = new("Part",char)
	Sbj5.Name = "Fusion Transformation Part 1"
	Sbj5.BrickColor = bc("Neon orange")
