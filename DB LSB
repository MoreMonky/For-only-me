					char.Pants.PantsTemplate = "http://www.roblox.com/asset/?id=4726887218"
				end

				if UltraInstinctMade == true and Base == true then
					Base = false

					B_anim = false
					B_animSSJBKK = true

					UITheme:Play()
					wait(5)
					CreateSound(2644268083, Head, 0.6, 1, 0, false, false, "GogetaUI")
					----
					for i = 1,1 do
						game:GetService("RunService").Heartbeat:wait()
						for i = 1,80 do
							game:GetService("RunService").Heartbeat:wait(0.00000001)

							a10.Color = a10.Color:lerp(Color3.fromRGB(0, 255, 255), 0.1)
							a11.Color = a11.Color:lerp(Color3.fromRGB(0, 255, 255), 0.1)
							a12.Color = a12.Color:lerp(Color3.fromRGB(0, 255, 255), 0.1)
