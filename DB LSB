end)


Event.OnServerEvent:Connect(function(plr,pos,key) -- Taunts

	if key == "c" and ssj1 == true then
		if Head:FindFirstChild("Yare Yare Daze...") then
			return
		end

		Taunt.SoundId = "rbxassetid://3173441553"
		Taunt:Play()

		CreateTextGui(Head, "NOW, IM REALLY MAD!", 3, Color3.fromRGB(255, 255, 0), Color3.fromRGB(0, 0, 0), Enum.Font.PermanentMarker)

