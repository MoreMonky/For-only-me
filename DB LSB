Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "b" and MasteredUltraInstinct == true then

		BRC = 0
		attack = true

		rarm.Transparency = 1
		larm.Transparency = 1
		a10.Transparency = 1
		a11.Transparency = 1

		LeftLow.Parent = a10b
		LeftLow2.Parent = a10b
		LeftLow3.Parent = a10b
		LeftLow5.Parent = a10b
		MUIParticle3.Parent = a10b


		LeftLeg90.Parent = a11b
		LeftLeg2.Parent = a11b
		LeftLeg3.Parent = a11b
		LeftLeg5.Parent = a11b
		MUIParticle4.Parent = a11b

		MUIParticle4.Enabled = false
		MUIParticle3.Enabled = false
		UltRarm.Parent = a11b
		UltLarm.Parent = a10b
		for i = 0, 1.12, 0.1 do
			swait()
			rootj.C0 = clerp(rootj.C0, RootCF * CFrame.new(0, -0.9, -0) * angles(math.rad(14), math.rad(0), math.rad(0)), 0.35)
			tors.Neck.C0 = clerp(tors.Neck.C0, necko * angles(math.rad(-0), math.rad(0), math.rad(-0)), 0.35)
			RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.6, -0.8) * angles(math.rad(110), math.rad(23), math.rad(2)), 0.4)
			LW.C0 = clerp(LW.C0, CFrame.new(-1.3, 0.6, -0.8) * angles(math.rad(110), math.rad(-23), math.rad(-2)), 0.35)
			RH.C0 = clerp(RH.C0, CFrame.new(1, -1, -0.3) * RHCF * angles(math.rad(-4), math.rad(0), math.rad(6)), 0.3)
			LH.C0 = clerp(LH.C0, CFrame.new(-1, -1, 0.05) * LHCF * angles(math.rad(-22), math.rad(0), math.rad(23)), 0.3)
		end


		while BRC <3 do

			wait(0.05)
			CreateEffect(rarm, "BarrageArm", rarm.Color)
			CreateEffect(larm, "BarrageArm", larm.Color)
			AoEpunch(root.CFrame * Vector3.new(0,0.5,-4),3.75)
			BRC = BRC + 0.1
		end
		attack = false
		rarm.Transparency = 0
		larm.Transparency = 0
		a10.Transparency = 0
		a11.Transparency = 0
		MUIParticle4.Enabled = true
		MUIParticle3.Enabled = true
		LeftLow.Parent = larm
		LeftLow2.Parent = larm
		LeftLow3.Parent = larm
		LeftLow5.Parent = larm


		UltRarm.Parent = rarm
		UltLarm.Parent = larm

		LeftLeg90.Parent = rarm
		LeftLeg2.Parent = rarm
		LeftLeg3.Parent = rarm
		LeftLeg5.Parent = rarm

		MUIParticle3.Parent = larm
		MUIParticle4.Parent = rarm
	end
end)


