			Sb.Transparency = 0		

			a8.Color = Color3.fromRGB(0, 0, 0)

			a8.Transparency = 0

			wait(0.8)



			for i = 1, 8 do
				local p = Instance.new("Part")
				p.CanCollide = false
				p.Anchored = true
				p.Material = "Neon"

				p.BrickColor = BrickColor.new("Olivine")


				p.Size = Vector3.new(0.5,3,0.5)
				p.CFrame = torso1.CFrame*randomangle()
				setshape(p, "Sphere")
				p.Parent = char
				spawn(function()
					local endcf = p.CFrame *CFrame.new(0,5,0)
					for i = 1, 50 do
						local cf = p.CFrame
						p.Size = p.Size:Lerp(Vector3.new(0.5,0.5,0.5), 0.1)
						p.CFrame = cf
						p.CFrame = cf:Lerp(endcf, 0.1)
						p.Transparency = i/50
						swait()
					end
					p:Destroy()
				end)
			end

			char.Torso.Zamasu:Destroy()




			B_animSSJB = false
			B_animGB = true

			ColorMode = 3
			--ZamasCheck
			wait(0.6)
			a10b.BrickColor = bc("Hot pink")
			a11b.BrickColor = bc("Hot pink")
			--a12b.BrickColor = bc("Cyan")
			a13b.BrickColor = bc("Hot pink")
			a14b.BrickColor = bc("Hot pink")
			a15b.BrickColor = bc("Hot pink")
			--a8b.BrickColor = bc("Cyan")
			for i = 1,15 do
				UILight2.Enabled = true
				a10b.Transparency = a10b.Transparency - 0.07
				a11b.Transparency = a11b.Transparency - 0.05
				--a12b.Transparency = a12b.Transparency - 0.05
				a13b.Transparency = a13b.Transparency - 0.07
				a14b.Transparency = a14b.Transparency - 0.07
				a15b.Transparency = a15b.Transparency - 0.07
				--	a8.Color = a8.Color:lerp(Color3.fromRGB(9, 137, 207), 0.1)

				wait()
			end

			a10b.Transparency = 1
			a11b.Transparency = 1
			a13b.Transparency = 1
			a14b.Transparency = 1
			a15b.Transparency = 1
			UILight2.Enabled = false
			EnergyExplosion(3)

			ColorMode = 1
