	ACTUALBALL903.Touched:Connect(function(hit)
		if hit.Parent ~= char then
			for i, v in pairs(hit.Parent:GetChildren()) do
				if v.Name == "Humanoid" then

					v.Health = v.Health - (v.MaxHealth/350)

				end
			end
		end
	end)

	for i = 1, 70 do
		wait()

		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(95),math.rad(-10),math.rad(-30)), 0.5)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(75),math.rad(10),math.rad(30)), 0.5)--arm
		lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
		lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
		Ball101.Size = Ball101.Size + Vector3.new(0,8,0)
		Ball102.Size = Ball102.Size + Vector3.new(0,8,0)
		Ball101.CFrame = Ball101.CFrame * CFrame.new(0,4,0)
		Ball102.CFrame = Ball101.CFrame
		ACTUALBALL903.CFrame = ACTUALBALL903.CFrame * CFrame.new(0,8,0)
		ACTUALBALL904.CFrame = ACTUALBALL903.CFrame
	end


	sound3:Destroy()
	sound1:Destroy()
	sound4:Destroy()
	for i = 1, 50 do
		wait()
		Ball1.Transparency = Ball1.Transparency + 0.02
		Ball2.Transparency = Ball2.Transparency + 0.02
		Ball101.Transparency = Ball101.Transparency + 0.02
		Ball102.Transparency = Ball102.Transparency + 0.02
		ACTUALBALL.Transparency = ACTUALBALL.Transparency + 0.02
		ACTUALBALL2.Transparency = ACTUALBALL2.Transparency + 0.02
		ACTUALBALL3.Transparency = ACTUALBALL3.Transparency + 0.02
		ACTUALBALL4.Transparency = ACTUALBALL4.Transparency + 0.02
		ACTUALBALL90.Transparency = ACTUALBALL90.Transparency + 0.02
		ACTUALBALL902.Transparency = ACTUALBALL902.Transparency + 0.02
		ACTUALBALL903.Transparency = ACTUALBALL903.Transparency + 0.02
		ACTUALBALL904.Transparency = ACTUALBALL904.Transparency + 0.02
	end
	Model:Destroy()
	Model2:Destroy()
	char.HumanoidRootPart.Anchored = false
end

function TestKame(color,Charge1,Fire1,volume,timeR)

	if Base == false and SSJBKaioken == false then
		char.HumanoidRootPart.Anchored = true
		local sound1 = Instance.new("Sound", char.HumanoidRootPart)
		sound1.SoundId = "rbxassetid://2889442449"
		sound1.Volume = 0.5
		sound1:Play()
		local sound2 = Instance.new("Sound", char.HumanoidRootPart)
		print(Charge1)
		if Charge1 == nil then
			sound2.SoundId = "rbxassetid://2890285817"
		else
			sound2.SoundId = Charge1
		end	
		if volume == nil then
			sound2.Volume = 1
		else
			sound2.Volume = volume
		end
		
		sound2:Play()
		local Part = Instance.new("Part", char)
		Part.Anchored = false
		Part.CanCollide = false
		Part.BrickColor = BrickColor.new("Institutional white")
		Part.Material = "Neon"
		Part.CastShadow = false
		Part.Size = Vector3.new(1.3,1.3,1.3)
		Part.Shape = "Ball"
		local Part199 = Instance.new("Part", Part)
		Part199.Anchored = false
		Part199.CanCollide = false
		
		if color == nil then
			Part199.BrickColor = BrickColor.new("Cyan")
		else
			Part199.BrickColor = color
		end
		
		Part199.Material = "Neon"
		Part199.CastShadow = false
		Part199.Size = Vector3.new(2,2,2)
		Part199.Transparency = 0.5
		Part199.Shape = "Ball"
		local weld = Instance.new("Weld", Part)
		weld.Part0 = char["Right Arm"]
		weld.Part1 = Part
		weld.C0 = CFrame.new(-0.35, -1, 0.35)
		local weld2 = Instance.new("Weld", Part)
		weld2.Part0 = Part
		weld2.Part1 = Part199
		weld2.C0 = CFrame.new(0,0,0)
		
		if timeR == nil then
			timeR = 50
		end
		
		for i = 1, timeR do
			wait()
			char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame:Lerp(CFrame.new(char.HumanoidRootPart.Position, CurrentPos),0.5)
			rootj.C0 = rootj.C0:Lerp(v41 * CFrame.new(0, 0, -0.2 + v2980 / 8) * CFrame.Angles(0.5, -0.3, -1.2), 0.3)
			RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(85),math.rad(-10),math.rad(-30)), 1)--arm
			LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(85),math.rad(10),math.rad(30)), 1)--arm
			RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(0, -0.5, 0.2), 0.3)
			LH.C0 = LH.C0:Lerp(CFrame.new(-0.7, -0.5 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.6, 1.3, 0) * CFrame.Angles(0.4, 0, 0), 0.3)
			tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.new(0, 0, 0) * CFrame.Angles(-0.25, -0.25, 1.2), 0.3)
			lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
			lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	

		end
		Part:Destroy()
		sound2.Ended:Connect(function()
			sound2:Destroy()
		end)
