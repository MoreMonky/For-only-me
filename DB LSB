	ACTUALBALL902.Transparency = 0.5
	local Ball101 = Instance.new("Part", Model2)
	Ball101.Anchored = true
	Ball101.CanCollide = false
	Ball101.BrickColor = BrickColor.new("Institutional white")
	Ball101.Material = "Neon"
	Ball101.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,0,-32) * CFrame.Angles(-33,0,0)
	Ball101.Size = Vector3.new(59,2,59)
	local Ball102 = Instance.new("Part", Model2)
	Ball102.Anchored = true
	Ball102.CanCollide = false
	Ball102.BrickColor = BrickColor.new("Cyan")
	Ball102.Material = "Neon"
	Ball102.CFrame = Ball101.CFrame
	Ball102.Size = Vector3.new(60,3,60)
	Ball102.Transparency = 0.5
	local MESH4 = Instance.new("CylinderMesh", Ball101)
	local MESH5 = Instance.new("CylinderMesh", Ball102)
	local ACTUALBALL903 = Instance.new("Part",Model2)
	ACTUALBALL903.Anchored = true
	ACTUALBALL903.CanCollide = false
	ACTUALBALL903.BrickColor = BrickColor.new("Institutional white")
