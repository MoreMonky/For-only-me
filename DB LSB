-------------------------------Dragon Fist------------------------------------------------

---------------------------------------------------------------------------------------

you = char.Torso

local tsteleportcooldown = false
function tsteleport()

	if tsteleportcooldown == true then return end
	local location = CurrentPos
	if (Character.HumanoidRootPart.Position - location).magnitude < 999 then

		tsteleportcooldown = true
		Character.HumanoidRootPart.CFrame = CFrame.new(location) + Vector3.new(0,7,0)
		CreateSound(5066021887, hed, 0.7, 1, 0.04, false, false, "Time Teleport SFX")

		wait(2)
		tsteleportcooldown = false
	end
end



Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "z" then
		if canattack == true then
			canidle = false
			CanFight = false
			canwalk = false
			attack = true
			for i = 1, 10 do
				wait()
				RW.C0 = Lerp2(RW.C0, CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(10)), 0.4)--arm
				LW.C0 = Lerp2(LW.C0, CFrame.new(-0.8,0.7,-0.7)*CFrame.Angles(math.rad(-10),math.rad(0),math.rad(140)), 0.4)--arm
				lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
				lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
				lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
				lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
				lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
				lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
			end
			canidle = false
			CanFight = true
			canwalk = true
			attack = false
			tsteleport()
			canidle = false
			CanFight = true
			canwalk = true
		end
	end
end)

function GenkiBall()
	coroutine.resume(coroutine.create(function()
		local magnitude = nil
		local Position = nil

		Position = rarm.Position

		local	Genki00 = Instance.new("Part")
		local	HighLightGenki00 = Instance.new("Highlight",Genki00)
		HighLightGenki00.FillTransparency = 1
		HighLightGenki00.DepthMode = "Occluded"
		HighLightGenki00.OutlineColor = Color3.fromRGB(255, 255, 255)

		Genki00.Name = "GenkiTest7"
		Genki00.Parent = owner.Character
		Genki00.CFrame = CFrame.new(-39.5565186, 0.5, -48.1201172, 1, 0, 0, 0, 1, 0, 0, 0, 1)
		Genki00.Position = Genki0.Position
		Genki00.Size = Genki0.Size
		Genki00.Transparency = 0.2
		Genki00.BottomSurface = Enum.SurfaceType.Smooth
		Genki00.BrickColor = BrickColor.new("Toothpaste")
		Genki00.Material = Enum.Material.Neon
		Genki00.TopSurface = Enum.SurfaceType.Smooth
		Genki00.brickColor = BrickColor.new("Toothpaste")
		Genki00.Shape = Enum.PartType.Ball

		local	Genki11 = Instance.new("Part")
		Genki11.Name = "GenkiTest9"
		Genki11.Parent = Genki00
		Genki11.CFrame = CFrame.new(-39.5565186, 0.5, -48.1201172, 1, 0, 0, 0, 1, 0, 0, 0, 1)
		Genki11.Position = Genki1.Position
		Genki11.Size = Genki1.Size
		Genki11.BottomSurface = Enum.SurfaceType.Smooth
		Genki11.BrickColor = BrickColor.new("Institutional white")
		Genki11.Material = Enum.Material.Neon
		Genki11.TopSurface = Enum.SurfaceType.Smooth
		Genki11.brickColor = BrickColor.new("Institutional white")
		Genki11.Shape = Enum.PartType.Ball
		Genki11.Transparency = 0

		local	aaa80907 =Instance.new("Weld",Genki00)
		aaa80907.Part0 = Genki11
		aaa80907.Part1 = Genki00

		Genki11.Massless = true

		Genki00.Anchored = false

		local bforce = Instance.new("BodyForce",Genki00)
		bforce.force = Vector3.new(0, ((bforce.Parent:getMass())*workspace.Gravity), 0)

		Genki00.CanCollide = false

		local Target = CurrentPos

		local direction = Target - Position
		local direction = computeDirection(direction)
		local pos = Position + (direction * 2) 
		Genki00.CFrame = CFrame.new(pos,  pos + direction) * CFrame.Angles((math.pi/2)+math.rad(180), 0, 0)
		Genki00.Velocity = direction * 150
		local asd = nil

	
	end))
end

function WhiteBall()
	coroutine.resume(coroutine.create(function()
		local magnitude = nil
		local Position = nil

		Position = rarm.Position

		local Part0 = Instance.new("Part",char)

		Part0.Name = "Bullet"
		Part0.Material = Enum.Material.Neon

		Part0.BrickColor = BrickColor.random()
	
		Part0.Anchored = false
		Part0.Size = Vector3.new(1, 1, 1)

		local mesh = Instance.new("SpecialMesh",Part0)
		mesh.MeshType = Enum.MeshType.Sphere
		mesh.Scale = Vector3.new(1,1,1)

		local bforce = Instance.new("BodyForce",Part0)
		bforce.force = Vector3.new(0, ((bforce.Parent:getMass())*workspace.Gravity), 0)
		Part0.CanCollide = false


		local Target = CurrentPos
		local direction = Target - Position
		local direction = computeDirection(direction)
		local pos = Position + (direction * 2) 

		Part0.CFrame = CFrame.new(pos,  pos + direction) * CFrame.Angles((math.pi/2)+math.rad(180), 0, 0)
		Part0.Velocity = direction * 150
		local asd = nil
		
	end))
end

local Cooldown = false
Event.OnServerEvent:Connect(function(plr,pos,key)
	if key == "j" and MasteredUltraInstinct == true and Cooldown == false then
		Cooldown = true
		HighSpeed()
		wait(2)
		Cooldown = false

	elseif key == "j" and FusedSSJ == true then
		canattack = false
		CanFight = false
		canwalk = false
		attack = true

		local HQ = Instance.new("Part",char)
		HQ.Size = Vector3.new(0.1,0.1,0.1)
		HQ.BrickColor = BrickColor.new("White")
		HQ.Material = "Neon"
		HQ.Anchored = true
		HQ.CanCollide = false
		HQ.CFrame = hed.CFrame*CFrame.new(2,1,0)*CFrame.Angles(0,0,0)

		local HQ2 = Instance.new("SpecialMesh",HQ)
		HQ2.MeshType = 3
		HQ2.Scale = Vector3.new(.1,.1,.1)

		local FZcharge3 = Instance.new("ParticleEmitter",HQ)
		FZcharge3.Texture = "rbxassetid://249481416"
		FZcharge3.Color = ColorSequence.new(Color3.fromRGB(255, 255, 255))
		FZcharge3.EmissionDirection = "Top"
		FZcharge3.Speed = NumberRange.new(6)
		FZcharge3.Size = NumberSequence.new(2.10,0)
		FZcharge3.Transparency = NumberSequence.new(0,1)
		FZcharge3.Drag = 1.2
		FZcharge3.LockedToPart = true
		FZcharge3.Lifetime = NumberRange.new(1)
		FZcharge3.Rate = 55
		FZcharge3.LightEmission = 0.3
		FZcharge3.Rotation = NumberRange.new(-5,5)
		FZcharge3.VelocitySpread = 0.2	
		FZcharge3.ZOffset = 0
		FZcharge3.Texture = "rbxassetid://249481416"
		FZcharge3.Speed = NumberRange.new(0)
		FZcharge3.Lifetime = NumberRange.new(0.2)
		FZcharge3.Rotation = NumberRange.new(-360,360)

		coroutine.resume(coroutine.create(function()
			for i = 1,20 do
				HQ2.Scale = HQ2.Scale + Vector3.new(0.5,0.5,0.5)
				HQ.BrickColor = BrickColor.random()
				HQ.CFrame = HQ.CFrame*CFrame.Angles(0,-0.2,0)

				wait(0.00000000001)
			end
		end))		

		for i = 1, 20 do

			RW.C0 = Lerp2(RW.C0, CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(-180),math.rad(0),math.rad(20)), 0.3)--rarm
			LW.C0 = Lerp2(LW.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)), 0.3)--larm
			wait()

		end

		wait(2)
		HQ:Destroy()

		for i = 1, 20 do
			wait()

			RW.C0 = Lerp2(RW.C0, CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(100),math.rad(0),math.rad(20)), 0.1)--rarm

		end

		WhiteBall()
		wait(0.5)

		canattack = true
		CanFight = true
		canwalk = true
		attack = false
	end
end)

