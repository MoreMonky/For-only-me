	ACTUALBALL3.Size = Vector3.new(41,41,41)
	ACTUALBALL3.Shape = "Ball"
	local ACTUALBALL4 = Instance.new("Part",Model)
	ACTUALBALL4.Anchored = true
	ACTUALBALL4.CanCollide = false
	ACTUALBALL4.BrickColor = BrickColor.new("Cyan")
	ACTUALBALL4.Material = "Neon"
