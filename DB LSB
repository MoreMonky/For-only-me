		ACTUALBALL3.Size = Vector3.new(5,5,5)
		ACTUALBALL3.Shape = "Ball"
		local ACTUALBALL4 = Instance.new("Part",Model)
		ACTUALBALL4.Anchored = true
		ACTUALBALL4.CanCollide = false
		ACTUALBALL4.BrickColor = BrickColor.new("Cyan")
		ACTUALBALL4.Material = "Neon"
		ACTUALBALL4.CFrame = ACTUALBALL3.CFrame
		ACTUALBALL4.Size = Vector3.new(6,6,6)
		ACTUALBALL4.Shape = "Ball"
		ACTUALBALL4.Transparency = 0.5
		Ball1.Touched:Connect(function(hit)
			if hit.Parent ~= char then
				for i, v in pairs(hit.Parent:GetChildren()) do
