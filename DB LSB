			GogetaLight2.Enabled = true
			a8.Color = a8.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a8b.Color = a8b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a10b.Color = a10b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a11b.Color = a11b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a12b.Color = a12b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a13b.Color = a13b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a14b.Color = a14b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			a15b.Color = a15b.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
			ape5.Color = ape5.Color:lerp(Color3.fromRGB(239, 184, 56), 0.1)
		end

	end
	wait(0.2)
	Thing:Destroy()
end

--------------------------------------

function endBigBangKamehameha()
	for i = 1, 10 do
		wait()	
		attack = true
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.1,0.7,-0.5)*CFrame.Angles(math.rad(95),math.rad(-10),math.rad(-30)), 0.5)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.1,0.3,-0.5)*CFrame.Angles(math.rad(75),math.rad(10),math.rad(30)), 0.5)--arm
		lerpz(RJ, "C0", RJC0 * cf(0, 0.25, -0.35) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(N, "C0", NC0 * cf(0, 0, 0) * ang(rd(0), rd(0), rd(0)), 0.5)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		lerpz(RH, "C0", RHC0 * cf(0, 0, 0.05) * ang(rd(-10), rd(-15), rd(0)), 0.5)	
		lerpz(LH, "C0", LHC0 * cf(-0, 0, 0.05) * ang(rd(-10), rd(15), rd(-0)), 0.5)
	end
end

function fatherson()
	fahersonend:Play()
	for i = 1,20 do	
		wait()


		--rootj.C0 = rootj.C0:Lerp(rootj.C0 * CFrame.Angles(0, -0, rad(180)), 0.2)
		RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.57, 0) * CFrame.Angles(0, -0.5, 0.2), 0.3)
		LH.C0 = LH.C0:Lerp(CFrame.new(-0.7, -0.5 - v2980 / 8, 0) * CFrame.Angles(0, -1.57, 0) * CFrame.Angles(-0.6, 1.3, 0) * CFrame.Angles(0.4, 0, 0), 0.3)
		tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.new(0, 0, 0) * CFrame.Angles(-0.25, -0.25, 1.2), 0.3)
		lerpz(RS, "C0", RSC0 * cf(0, 0, 0) * ang(rd(-5), rd(5), rd(45)), 0.5)	
		lerpz(LS, "C0", LSC0 * cf(0, 0, 0) * ang(rd(-5), rd(-5), rd(-45)), 0.5)	
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.5,0.5,0.5)*CFrame.Angles(math.rad(-65),math.rad(0),math.rad(10)), .1)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(10),math.rad(0),math.rad(-20)), .1)--arm
	end
end
function fathersonfire()
	for i = 1,10 do
		wait()
		tors.Neck.C0 = tors.Neck.C0:Lerp(v36 * CFrame.Angles(0, 0, rad(0)), 0.2)
		--rootj.C0 = rootj.C0:Lerp(rootj.C0 * CFrame.Angles(-0, 0,0), 0.2)
		RH.C0 = RH.C0:Lerp(CFrame.new(1, -1 - v2980 / 8, 0) * CFrame.Angles(0, 1.6, 0) * CFrame.Angles(-0.1, -0.25, 0), 0.2)
		LH.C0 = LH.C0:Lerp(CFrame.new(-1, -1 - v2980 / 8, -0) * CFrame.Angles(0, -1.6, 0) * CFrame.Angles(-0.1, 0.5, -0), 0.2)
		RW.C0 = Lerp2(RW.C0, CFrame.new(1.2,0.5,-0.3)*CFrame.Angles(math.rad(90),math.rad(0),math.rad(0)), .4)--arm
		LW.C0 = Lerp2(LW.C0, CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(-20),math.rad(0),math.rad(-10)), .4)--arm
	end
end
