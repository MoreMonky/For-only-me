					wait(0.2)
					aaaae.Enabled = false
					B_animSSJG = true

					RightAura3.Enabled = true
					RightLow3.Enabled = true
					LeftLow3.Enabled = true
					HairAura3.Enabled = true
					LeftLeg3.Enabled = true

				end

				wait(0.3)
				aaaae.Size = ns(20)
				ssjaura:Stop()
				VFace.Texture = "rbxassetid://1644662662"
				
				aaaae.Enabled = true
				wait(0.2)
				aaaae.Enabled = false

				RightAura3.Enabled = true
				RightLow3.Enabled = true
				LeftLow3.Enabled = true
				HairAura3.Enabled = true
				LeftLeg3.Enabled = true

				if SuperSaiyan == true then

					aaaae.Enabled = false

					RightAura3.Enabled = false
					RightLow3.Enabled = false

					LeftLow3.Enabled = false

					B_anim = true
					B_animSSJG = false
					HairAura3.Enabled = false
					LeftLeg3.Enabled = false

				end

				ssjaura:Stop()
				
				

				aa8.Offset = Vector3.new(0.2, -0.2, 0.3	)
				aa8.MeshId = "rbxassetid://430344159"
				aa8.Scale = Vector3.new(6.2, 5.5, 6.2)
				aaa8.C0 = CFrame.new(-.25, 1.2, .34)

				a8b.BrickColor = bc("Really black")
				a8b.Transparency = 0
				aa8b.Offset = Vector3.new(0.2, -0.2, 0.3)
				aa8b.Scale = v3(1.026,1.026,1.026)
				aa8b.MeshId = "rbxassetid://8245878571"
				aaa8b.C0 = CFrame.new(-.25, 1.2, .34)
				
				for i = 1,1 do
					game:GetService("RunService").Heartbeat:wait()
					for i = 1,80 do
						game:GetService("RunService").Heartbeat:wait(0.00000001)

						a8.Color = a8.Color:lerp(Color3.fromRGB(255, 255, 0), 0.1)
					end

					for i = 1,30 do
						game:GetService("RunService").Heartbeat:wait(0.00000001)


					end
				end

				wait(0.5)

				Transforming = false
				TextLabel4.Text = "Super Saiyan"
				TextLabel4.TextColor = BrickColor.new("Tr. Flu. Yellow")
				TextLabel4.TextColor3 = Color3.new(1, 0.984314, 0.490196)

				TextLabel11.Text = "Y - Super Saiyan 2"
				TextLabel12.Text = "-"
				TextLabel13.Text = "X - Base"
				TextLabel16.Text = "-"
				TextLabel17.Text = "H - Ultimate"
				TextLabel18.Text = "-"
				TextLabel19.Text = "-"
				TextLabel20.Text = "-"
				-------------------------------------------------
				canattack = true
				CanFight = true
				canwalk = true
				hum.WalkSpeed = 16

				wait(0.6)

				ssj1 = true
				SuperSaiyan = true


			end
		end
	end
end)


